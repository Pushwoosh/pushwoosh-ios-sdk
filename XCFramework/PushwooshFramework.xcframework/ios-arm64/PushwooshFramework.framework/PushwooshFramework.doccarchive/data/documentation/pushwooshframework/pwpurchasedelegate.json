lete(_ identifier: String?) {","        guard let identifier = identifier else { return }","","        Analytics.log(\"purchase_complete\", [\"product\": identifier])","        PremiumManager.shared.unlock(productId: identifier)","    }","","    func onPWInAppPurchaseHelperPaymentFailed(productIdentifier identifier: String?, error: Error?) {","        Analytics.log(\"purchase_failed\", [","            \"product\": identifier ?? \"\",","            \"error\": error?.localizedDescription ?? \"\"","        ])","    }","","    func onPWInAppPurchaseHelperCallPromotedPurchase(_ identifier: String?) {","        guard let identifier = identifier else { return }","","        showPurchaseConfirmation(for: identifier)","    }","","    func onPWInAppPurchaseHelperRestoreCompletedTransactionsFailed(_ error: Error?) {","        showError(\"Failed to restore purchases: \\(error?.localizedDescription ?? \"Unknown error\")\")","    }","}"],"syntax":"swift","type":"codeListing"}],"kind":"content"}],"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWPurchaseDelegate","interfaceLanguage":"swift"},"variants":[{"paths":["\/documentation\/pushwooshframework\/pwpurchasedelegate"],"traits":[{"interfaceLanguage":"swift"}]},{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshframework\/pwpurchasedelegate"]}],"topicSections":[{"identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWPurchaseDelegate\/onPW(inAppPurchaseHelperCallPromotedPurchase:)"],"title":"Purchase Events"},{"title":"Instance Methods","identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWPurchaseDelegate\/onPW(inAppPurchaseHelperPaymentComplete:)","doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWPurchaseDelegate\/onPW(inAppPurchaseHelperPaymentFailedProductIdentifier:error:)","doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWPurchaseDelegate\/on