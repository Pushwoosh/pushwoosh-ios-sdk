ication,","                     didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Pushwoosh.TVoS.handleTvPushToken(deviceToken)","    }","","    func application(_ application: UIApplication,","                     didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Pushwoosh.TVoS.handleTvPushRegistrationFailure(error)","    }","","    func application(_ application: UIApplication,","                     didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                     fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","    }","}"],"type":"codeListing"},{"anchor":"Rich-Media-Configuration","text":"Rich Media Configuration","type":"heading","level":2},{"level":3,"type":"heading","anchor":"Center-Position-with-Bottom-Animation","text":"Center Position with Bottom Animation"},{"type":"paragraph","inlineContent":[{"text":"Display Rich Media in the center with slide-in animation from bottom:","type":"text"}]},{"type":"codeListing","code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Configure Rich Media","    Pushwoosh.TVoS.configureRichMediaWith(","        position: .center,","        presentAnimation: .fromBottom,","        dismissAnimation: .toBottom","    )","","    Pushwoosh.TVoS.configureCloseButton(true)","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"],"syntax":"swift"},{"level":3,"anchor":"Left-Side-Panel","type":"heading","text":"Left Side Panel"},{"type":"paragraph","inlineContent":[{"text":"Display Rich Media as a left-side panel:","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .left,","    presentAnimation: .fromLeft,","    dismissAnimation: .toLeft",")"]},{"level":3,"type":"heading","text":"Top Banner","anchor":"Top-Banner"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Display Rich Media as a top banner with slide-down animation:"}]},{"type":"codeListing","syntax":"swift","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .top,","    presentAnimation: .fromTop,","    dismissAnimation: .toTop",")"]},{"level":3,"anchor":"No-Close-Button","text":"No Close Button","type":"heading"},{"inlineContent":[{"type":"text","text":"Hide the Close button if your Rich Media has custom dismiss logic:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .center,","    presentAnimation: .fromBottom,","    dismissAnimation: .toBottom",")","","Pushwoosh.TVoS.configureCloseButton(false)"]},{"level":2,"anchor":"Custom-Push-Handling","type":"heading","text":"Custom Push Handling"},{"level":3,"text":"Selective Rich Media Display","type":"heading","anchor":"Selective-Rich-Media-Display"},{"type":"paragraph","inlineContent":[{"text":"Display Rich Media only for specific push types:","type":"text"}]},{"type":"codeListing","code":["func application(_ application: UIApplication,","                 didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                 fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","","    \/\/ Check if push contains Rich Media","    if let rm = userInfo[\"rm\"] as? [String: Any],","       let url = rm[\"url\"] as? String {","","        \/\/ Check URL or other criteria","        if url.contains(\"special-offer\") {","            \/\/ Display Rich Media","            if Pushwoosh.TVoS.handleTVOSPush(userInfo: userInfo) {","                completionHandler(.newData)","                return","            }","        }","    }","","    \/\/ Handle as regular push","    Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","}"],"syntax":"swift"},{"type":"heading","level":2,"anchor":"Registration-with-Completion-Handler","text":"Registration with Completion Handler"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Register with device token and handle success\/failure:"}]},{"syntax":"swift","type":"codeListing","code":["func application(_ application: UIApplication,","                 didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","    Pushwoosh.TVoS.registerForTvPushNotifications(withToken: deviceToken) { error in","        if let error = error {","            print(\"Failed to register: \\(error.localizedDescription)\")","        } else {","            print(\"Successfully registered for push notifications\")","        }","    }","}"]},{"text":"Unregister from Push Notifications","type":"heading","level":2,"anchor":"Unregister-from-Push-Notifications"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Unregister the device from receiving push notifications:"}]},{"code":["Pushwoosh.TVoS.unregisterForTvPushNotifications { error in","    if let error = error {","        print(\"Failed to unregister: \\(error.localizedDescription)\")","    } else {","        print(\"Successfully unregistered from push notifications\")","    }","}"],"type":"codeListing","syntax":"swift"},{"level":2,"anchor":"getTags-Handler","text":"getTags Handler","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Handle tags retrieved from Rich Media:","type":"text"}]},{"code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Set getTags handler","    Pushwoosh.TVoS.setRichMediaGetTagsHandler { tags in","        print(\"Received tags from Rich Media:\")","        for (key, value) in tags {","           