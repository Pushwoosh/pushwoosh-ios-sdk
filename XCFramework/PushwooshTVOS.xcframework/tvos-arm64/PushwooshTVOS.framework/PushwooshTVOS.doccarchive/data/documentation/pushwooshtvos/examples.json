{"abstract":[{"type":"text","text":"Code samples demonstrating common tvOS integration scenarios."}],"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/PushwooshTVOS"]]},"metadata":{"modules":[{"name":"PushwooshTVOS"}],"role":"article","roleHeading":"Article","title":"Examples"},"primaryContentSections":[{"kind":"content","content":[{"type":"heading","text":"Overview","level":2,"anchor":"Overview"},{"inlineContent":[{"text":"This page provides ready-to-use code examples for integrating Pushwoosh into your tvOS application. Examples cover basic setup, Rich Media configuration, custom push handling, and advanced scenarios.","type":"text"}],"type":"paragraph"},{"text":"Basic Setup","level":2,"anchor":"Basic-Setup","type":"heading"},{"text":"Minimal AppDelegate Setup","anchor":"Minimal-AppDelegate-Setup","level":3,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The simplest way to integrate Pushwoosh into your tvOS app:"}]},{"type":"codeListing","syntax":"swift","code":["import UIKit","import Pushwoosh","import PushwooshTVOS","","@main","class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                     didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","        Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","        \/\/ Register for push notifications","        Pushwoosh.TVoS.registerForTvPushNotifications()","","        return true","    }","","    func application(_ application: UIApplication,","                     didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Pushwoosh.TVoS.handleTvPushToken(deviceToken)","    }","","    func application(_ application: UIApplication,","                     didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Pushwoosh.TVoS.handleTvPushRegistrationFailure(error)","    }","","    func application(_ application: UIApplication,","                     didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                     fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","    }","}"]},{"anchor":"Rich-Media-Configuration","text":"Rich Media Configuration","type":"heading","level":2},{"level":3,"type":"heading","anchor":"Center-Position-with-Bottom-Animation","text":"Center Position with Bottom Animation"},{"inlineContent":[{"text":"Display Rich Media in the center with slide-in animation from bottom:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Configure Rich Media","    Pushwoosh.TVoS.configureRichMediaWith(","        position: .center,","        presentAnimation: .fromBottom,","        dismissAnimation: .toBottom","    )","","    Pushwoosh.TVoS.configureCloseButton(true)","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"]},{"type":"heading","level":3,"text":"Left Side Panel","anchor":"Left-Side-Panel"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Display Rich Media as a left-side panel:"}]},{"syntax":"swift","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .left,","    presentAnimation: .fromLeft,","    dismissAnimation: .toLeft",")"],"type":"codeListing"},{"level":3,"anchor":"Top-Banner","text":"Top Banner","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Display Rich Media as a top banner with slide-down animation:","type":"text"}]},{"code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .top,","    presentAnimation: .fromTop,","    dismissAnimation: .toTop",")"],"type":"codeListing","syntax":"swift"},{"level":3,"type":"heading","anchor":"No-Close-Button","text":"No Close Button"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Hide the Close button if your Rich Media has custom dismiss logic:"}]},{"type":"codeListing","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .center,","    presentAnimation: .fromBottom,","    dismissAnimation: .toBottom",")","","Pushwoosh.TVoS.configureCloseButton(false)"],"syntax":"swift"},{"text":"Custom Push Handling","anchor":"Custom-Push-Handling","type":"heading","level":2},{"text":"Selective Rich Media Display","type":"heading","level":3,"anchor":"Selective-Rich-Media-Display"},{"inlineContent":[{"type":"text","text":"Display Rich Media only for specific push types:"}],"type":"paragraph"},{"syntax":"swift","code":["func application(_ application: UIApplication,","                 didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                 fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","","    \/\/ Check if push contains Rich Media","    if let rm = userInfo[\"rm\"] as? [String: Any],","       let url = rm[\"url\"] as? String {","","        \/\/ Check URL or other criteria","        if url.contains(\"special-offer\") {","            \/\/ Display Rich Media","            if Pushwoosh.TVoS.handleTVOSPush(userInfo: userInfo) {","                completionHandler(.newData)","                return","            }","        }","    }","","    \/\/ Handle as regular push","    Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","}"],"type":"codeListing"},{"text":"Registration with Completion Handler","anchor":"Registration-with-Completion-Handler","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Register with device token and handle success\/failure:"}]},{"type":"codeListing","syntax":"swift","code":["func application(_ application: UIApplication,","                 didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","    Pushwoosh.TVoS.registerForTvPushNotifications(withToken: deviceToken) { error in","        if let error = error {","            print(\"Failed to register: \\(error.localizedDescription)\")","        } else {","            print(\"Successfully registered for push notifications\")","        }","    }","}"]},{"type":"heading","level":2,"text":"Unregister from Push Notifications","anchor":"Unregister-from-Push-Notifications"},{"inlineContent":[{"text":"Unregister the device from receiving push notifications:","type":"text"}],"type":"paragraph"},{"code":["Pushwoosh.TVoS.unregisterForTvPushNotifications { error in","    if let error = error {","        print(\"Failed to unregister: \\(error.localizedDescription)\")","    } else {","        print(\"Successfully unregistered from push notifications\")","    }","}"],"syntax":"swift","type":"codeListing"},{"text":"getTags Handler","type":"heading","anchor":"getTags-Handler","level":2},{"inlineContent":[{"text":"Handle tags retrieved from Rich Media:","type":"text"}],"type":"paragraph"},{"code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Set getTags handler","    Pushwoosh.TVoS.setRichMediaGetTagsHandler { tags in","        print(\"Received tags from Rich Media:\")","        for (key, value) in tags {","            print(\"  \\(key): \\(value)\")","        }","    }","","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"],"type":"codeListing","syntax":"swift"}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/PushwooshTVOS\/Examples"},"seeAlsoSections":[{"identifiers":["doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/