":"swift","code":["Pushwoosh.ForegroundPush.useLiquidView = true"]},{"type":"paragraph","inlineContent":[{"type":"image","identifier":"ios-foreground-custom-5.gif"}]},{"name":"Note","style":"note","type":"aside","content":[{"inlineContent":[{"type":"strong","inlineContent":[{"text":"Important","type":"text"}]},{"type":"text","text":": Liquid Glass requires iOS 26+ and Swift 5.13+. On earlier iOS versions, standard UIView-based notifications are displayed. Swift versions below 5.13 use blurred UIVisualEffectView."}],"type":"paragraph"}]},{"level":2,"text":"Haptic Feedback Options","type":"heading","anchor":"Haptic-Feedback-Options"},{"level":3,"type":"heading","anchor":"No-Vibration","text":"No Vibration"},{"inlineContent":[{"text":"For silent notifications:","type":"text"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["Pushwoosh.ForegroundPush.foregroundNotificationWith(","    style: .style1,","    duration: 5,","    vibration: .none,","    disappearedPushAnimation: .balls",")"]},{"level":3,"type":"heading","text":"Strong Haptic","anchor":"Strong-Haptic"},{"type":"paragraph","inlineContent":[{"type":"text","text":"For important notifications:"}]},{"syntax":"swift","type":"codeListing","code":["Pushwoosh.ForegroundPush.foregroundNotificationWith(","    style: .style1,","    duration: 7,","    vibration: .heavy,","    disappearedPushAnimation: .balls",")"]},{"text":"System Notification Haptic","anchor":"System-Notification-Haptic","type":"heading","level":3},{"inlineContent":[{"type":"text","text":"Use system notification-style haptic:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["Pushwoosh.ForegroundPush.foregroundNotificationWith(","    style: .style1,","    duration: 5,","    vibration: .notification,","    disappearedPushAnimation: .balls",")"]},{"text":"Handling User Interactions","type":"heading","level":2,"anchor":"Handling-User-Interactions"},{"type":"heading","level":3,"anchor":"Implementing-Delegate","text":"Imp