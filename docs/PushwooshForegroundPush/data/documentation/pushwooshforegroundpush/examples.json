ic","type":"heading"},{"inlineContent":[{"type":"text","text":"Use system notification-style haptic:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["Pushwoosh.ForegroundPush.foregroundNotificationWith(","    style: .style1,","    duration: 5,","    vibration: .notification,","    disappearedPushAnimation: .balls",")"]},{"text":"Handling User Interactions","anchor":"Handling-User-Interactions","type":"heading","level":2},{"type":"heading","level":3,"text":"Implementing Delegate","anchor":"Implementing-Delegate"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handle user taps through the delegate method:"}]},{"type":"codeListing","syntax":"swift","code":["class AppDelegate: UIResponder, UIApplicationDelegate, PWForegroundPushDelegate {","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?","    ) -> Bool {","        Pushwoosh.ForegroundPush.delegate = self","        return true","    }","","    func didTapForegroundPush(_ userInfo: [AnyHashable: Any]) {","        print(\"Foreground custom push tapped: \\(userInfo)\")","","        if let customData = userInfo[\"custom\"] as? [String: Any],","           let screenId = customData[\"screen_id\"] as? String {","            navigateToScreen(screenId)","        }","    }","}"]},{"type":"heading","anchor":"Deep-Link-Handling","level":3,"text":"Deep Link Handling"},{"inlineContent":[{"type":"text","text":"Handle deep links from push notifications:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["func didTapForegroundPush(_ userInfo: [AnyHashable: Any]) {","    if let deepLink = userInfo[\"deep_link\"] as? String,","       let url = URL(string: deepLink) {","        UIApplication.shared.open(url, options: [:]) { success in","            print(\"Deep link opened: \\(success)\")","        }","    }","}"]},{"type":"heading","text":"Complete Custom Setup","anchor":"Complete-Custom-Setup","level":2},{"text":"Branded Notification","level":3,"anchor":"Branded-Notification","type":"heading"},{"inlineContent":[{"text":"Full customization with brand identity:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["func configureBrandedForegroundPush() {","    Pushwoosh.ForegroundPush.gradientColors = [","        UIColor(red: 0.18, green: 0.31, blue: 0.64, alpha: 1.0),","        UIColor(red: 0.44, green: 0.26, blue: 0.63, alpha: 1.0)","    ]","","    Pushwoosh.ForegroundPush.titlePushColor = .white","    Pushwoosh.ForegroundPush.messagePushColor = UIColor(white: 0.95, alpha: 1.0)","","    Pushwoosh.ForegroundPush.titlePushFont = UIFont(name: \"Helvetica-Bold\", size: 18)","    Pushwoosh.ForegroundPush.messagePushFont = UIFont(name: \"Helvetica\", size: 15)","","    Pushwoosh.ForegroundPush.usePushAnimation = true","    Pushwoosh.ForegroundPush.useLiquidView = true","","    Pushwoosh.ForegroundPush.foregroundNotificationWith(","        style: .style1,","        duration: 6,","        vibration: .notification,","        disappearedPushAnimation: .balls","    )","","    Pushwoosh.ForegroundPush.delegate = self","}"]},{"anchor":"Dark-Mode-Support","type":"heading","text":"Dark Mode Support","level":2},{"anchor":"Adaptive-Colors","text":"Adaptive Colors","level":3,"type":"heading"},{"inlineContent":[{"type":"text","text":"Adjust colors based on user interface style:"}],"type":"paragraph"},{"type":"codeListing","code":["func configureForegroundPushColors() {","    if #available(iOS 13.0, *) {","        let isDarkMode = UITraitCollection.current.userInterfaceStyle == .dark","","        if isDarkMode {","            Pushwoosh.ForegroundPush.gradientColors = [","                UIColor(red: 0.15, green: 0.15, blue: 0.15, alpha: 1.0),","                UIColor(red: 0.25, green: 0.25, blue: 0.25, alpha: 1.0)","            ]","            Pushwoosh.ForegroundPush.titlePushColor = .white","            Pushwoosh.Foreground