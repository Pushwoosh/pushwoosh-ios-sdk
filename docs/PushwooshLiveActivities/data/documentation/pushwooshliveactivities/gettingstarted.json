oshLiveActivityContentState {","        var status: String","        var estimatedTime: String","        var emoji: String","        var pushwoosh: PushwooshLiveActivityContentStateData?","    }","","    var orderNumber: String","    var pushwoosh: PushwooshLiveActivityAttributeData","}"],"type":"codeListing"},{"level":2,"type":"heading","text":"Step 5: Create Widget","anchor":"Step-5-Create-Widget"},{"inlineContent":[{"type":"text","text":"In your Widget Extension:"}],"type":"paragraph"},{"type":"codeListing","code":["import WidgetKit","import SwiftUI","import ActivityKit","","@main","struct FoodDeliveryWidget: Widget {","    var body: some WidgetConfiguration {","        ActivityConfiguration(for: FoodDeliveryAttributes.self) { context in","            VStack(alignment: .leading, spacing: 8) {","                HStack {","                    Text(\"Order #\\(context.attributes.orderNumber)\")","                        .font(.headline)","                    Spacer()","                    Text(context.state.emoji)","                        .font(.title)","                }","","                Text(context.state.status)","                    .font(.subheadline)","","                HStack {","                    Image(systemName: \"clock\")","                    Text(context.state.estimatedTime)","                }","                .font(.caption)","            }","            .padding()","        } dynamicIsland: { context in","            DynamicIsland {","                DynamicIslandExpandedRegion(.leading) {","                    Text(context.state.emoji)","                        .font(.title)","                }","                DynamicIslandExpandedRegion(.trailing) {","                    Text(context.state.estimatedTime)","                        .font(.caption)","                }","                DynamicIslandExpandedRegion(.center) {","                    Text(context.state.status)","                        .font(.caption)","                }","            } compactLeading: {","                Text(context.state.emoji)","            } compactTrailing: {","                Text(context.state.estimatedTime)","                    .font(.caption2)","            } minimal: {","                Text(context.state.emoji)","            }","        }","    }","}"],"syntax":"swift"},{"type":"heading","anchor":"Step-6-Initialize-in-AppDelegate","text":"Step 6: Initialize in AppDelegate","level":2},{"type":"codeListing","syntax":"swift","code":["import UIKit","import PushwooshFramework","import PushwooshLiveActivities","","@main","class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?","    ) -> Bool {","","        if #available(iOS 16.1, *) {","            Pushwoosh.LiveActivities.setup(FoodDeliveryAttributes.self)","        }","","        return true","    }","}"]},{"type":"paragraph","inlineContent":[{"text":"The ","type":"text"},{"code":"setup()","type":"codeVoice"},{"text":" method handles the entire lifecycle of Live Activities:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"text":"Automatically listens for push-to-start token updates (iOS 17.2+)","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Automatically listens for activity token updates"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Registers tokens with Pushwoosh server"}],"type":"paragraph"}]}]},{"text":"Step 7: Create Live Activity Manager","anchor":"Step-7-Create-Live-Activity-Manager","type":"heading","level":2},{"type":"codeListing","syntax":"swift","code":["import Foundation","import ActivityKit","import UIKit","import PushwooshFramework","import PushwooshLiveActivities","","class LiveActivityManager: NSObject, ObservableObject {","    public static let shared = LiveActivityManager()","    private var currentActivity: Activity<FoodDeliveryAttributes>?","","    func startActivity() {","        guard ActivityAuthorizationInfo().areActivitiesEnabled else {","            print(\"Live Activities are not enabled\")","            return","        }","","        do {","            let pushwooshData = PushwooshLiveActivityAttributeData(","                activityId: \"order_123\"","            )","","            let attribute = FoodDeliveryAttributes(","                orderNumber: \"1234567\",","                pushwoosh: pushwooshData","            )","","            let initialState = FoodDeliveryAttributes.ContentState(","                status: \"Preparing your meal\",","                estimatedTime: \"25 min\",","                emoji: \"üë®‚Äçüç≥\",","                pushwoosh: nil","            )","","            let activity = try Activity<FoodDeliveryAttributes>.request(","                attributes: attribute,","                content: .init(state: initialState, staleDate: nil),","                pushType: .token","            )","","            self.currentActivity = activity","","            Task {","                for await pushToken in activity.pushTokenUpdates {","                    let pushTokenString = pushToken.reduce(\"\") {","                        $0 + String(format: \"%02x\", $1)","                    }","","                    print(\"Activity push token: \\(pushTokenString)\")","","                    Pushwoosh.LiveActivities.startLiveActivity(","                        token: pushTokenString,","                        activityId: \"order_123\"","                    )","                }","            }","        } catch {","            print(\"Start Activity Error: \\(error.localizedDescription)\")","        }","    }","","    func endActivity() async {","        guard let activity = currentActivity else { return }","","        await activity.end(nil, dismissalPolicy: .immediate)","","        Pushwoosh.LiveActivities.stopLiveActivity(activityId: \"order_123\")","    }","}"]},{"anchor":"Step-8-Start-Activity-from-UI","level":2,"type":"heading","text":"Step 8: Start Activity from UI"},{"code":["import SwiftUI","","struct ContentView: View {","    var body: some View {","        VStack(spacing: 20) {","            Button(action: {","                LiveActivityManager.shared.startActivity()","            }) {","                Text(\"Start Live Activity\")","                    .foregroundColor(.white)","                    .padding()","                    .background(Color.blue)","                    .cornerRadius(10)","            }","","            Button(action: {","                Task {","                    await LiveActivityManager.shared.endActivity()","                }","            }) {","                Text(\"End Live Activity\")","                    .foregroundColor(.white)","                    .padding()","                    .background(Color.red)","                    .cornerRadius(10)","            }","        }","        .padding()","    }","}"],"syntax":"swift","type":"codeListing"},{"anchor":"Step-9-Send-Push-Notifications-via-Pushwoosh-API","level":2,"type":"heading","text":"Step 9: Send Push Notifications via Pushwoosh API"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Use Pushwoosh REST API to send Live Activity updates from your server."}]},{"anchor":"Start-Live-Activity-Push-to-Start-for-iOS-172+","type":"heading","level":3,"text":"Start Live Activity (Push-to-Start for iOS 17.2+)"},{"inlineContent":[{"type":"codeVoice","code":"POST https:\/\/api.pushwoosh.com\/json\/1.3\/startLiveActivity"}],"type":"paragraph"},{"syntax":"json","type":"codeListing","code":["{","  \"request\": {","    \"application\": \"YOUR-APP-CODE\",","    \"auth\": \"YOUR-API-TOKEN\",","    \"notifications\": [","      {","        \"content\": \"Your order is being prepared!\",","        \"live_activity\": {","          \"event\": \"start\",","          \"content-state\": {","