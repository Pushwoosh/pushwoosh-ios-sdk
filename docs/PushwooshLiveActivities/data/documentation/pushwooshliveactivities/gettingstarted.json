eActivityAttributes {","    public struct ContentState: PushwooshLiveActivityContentState {","        var status: String","        var estimatedTime: String","        var emoji: String","        var pushwoosh: PushwooshLiveActivityContentStateData?","    }","","    var orderNumber: String","    var pushwoosh: PushwooshLiveActivityAttributeData","}"],"type":"codeListing","syntax":"swift"},{"level":2,"text":"Step 5: Create Widget","anchor":"Step-5-Create-Widget","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"In your Widget Extension:"}]},{"type":"codeListing","syntax":"swift","code":["import WidgetKit","import SwiftUI","import ActivityKit","","@main","struct FoodDeliveryWidget: Widget {","    var body: some WidgetConfiguration {","        ActivityConfiguration(for: FoodDeliveryAttributes.self) { context in","            VStack(alignment: .leading, spacing: 8) {","                HStack {","                    Text(\"Order #\\(context.attributes.orderNumber)\")","                        .font(.headline)","                    Spacer()","                    Text(context.state.emoji)","                        .font(.title)","                }","","                Text(context.state.status)","                    .font(.subheadline)","","                HStack {","                    Image(systemName: \"clock\")","                    Text(context.state.estimatedTime)","                }","                .font(.caption)","            }","            .padding()","        } dynamicIsland: { context in","            DynamicIsland {","                DynamicIslandExpandedRegion(.leading) {","                    Text(context.state.emoji)","                        .font(.title)","                }","                DynamicIslandExpandedRegion(.trailing) {","                    Text(context.state.estimatedTime)","                        .font(.caption)","                }","                DynamicIslandExpandedRegion(.center) {","                    Text(context.state.status)","                        .font(.caption)","                }","            } compactLeading: {","                Text(context.state.emoji)","            } compactTrailing: {","                Text(context.state.estimatedTime)","                    .font(.caption2)","            } minimal: {","                Text(context.state.emoji)","            }","        }","    }","}"]},{"anchor":"Step-6-Initialize-in-AppDelegate","text":"Step 6: Initialize in AppDelegate","type":"heading","level":2},{"code":["import UIKit","import PushwooshFramework","import PushwooshLiveActivities","","@main","class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?","    ) -> Bool {","","        if #available(iOS 16.1, *) {","            Pushwoosh.LiveActivities.setup(FoodDeliveryAttributes.self)","        }","","        return true","    }","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"text":"The ","type":"text"},{"type":"codeVoice","code":"setup()"},{"type":"text","text":" method handles the entire lifecycle of Live Activities:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Automatically listens for push-to-start token updates (iOS 17.2+)"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Automatically listens for activity token updates"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Registers tokens with Pushwoosh server","type":"text"}]}]}]},{"text":"Step 7: Create Live Activity Manager","type":"heading","level":2,"anchor":"Step-7-Create-Live-Activity-Manager"},{"type":"codeListing","code":["import Foundation","import ActivityKit","import UIKit","import PushwooshFramework","import PushwooshLiveActivities","","class LiveActivityManager: NSObject, ObservableObject 