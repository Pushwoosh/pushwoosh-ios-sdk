ken"}]},{"text":": The push token string if registration succeeded, or ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" if it failed","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"error"}],"type":"strong"},{"type":"text","text":": An error object if registration failed, or "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if it succeeded"}]}]}]},{"level":2,"anchor":"Example","text":"Example","type":"heading"},{"inlineContent":[{"type":"text","text":"Handle registration result with UI updates and backend sync:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"]}]}],"schemaVersion":{"minor":3,"major":0,"patch":0},"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshRegistrationHandler":{"title":"PushwooshRegistrationHandler","role":"symbol","fragments":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}],"type":"topic","abstract":[{"type":"text","text":"Completion handler called when push notification regis