{"schemaVersion":{"patch":0,"major":0,"minor":3},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"typedef","kind":"keyword"},{"text":" ","kind":"text"},{"text":"void (^)(NSString *, NSError *)","kind":"typeIdentifier","preciseIdentifier":"c:BFv(#*$objc(cs)NSString#*$objc(cs)NSError)"},{"text":" ","kind":"text"},{"kind":"identifier","text":"PushwooshRegistrationHandler"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}]},{"content":[{"level":2,"type":"heading","text":"Overview","anchor":"Overview"},{"inlineContent":[{"type":"text","text":"This closure is called when the device registration process finishes, either successfully or with an error."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"The handler receives two parameters:","type":"text"}]},{"items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"token"}]},{"text":": The push token string if registration succeeded, or ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" if it failed","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"inlineContent":[{"text":"error","type":"text"}],"type":"strong"},{"type":"text","text":": An error object if registration failed, or "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if it succeeded"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"heading","text":"Example","anchor":"Example","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handle registration result with UI updates and backend sync:"}]},{"code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        