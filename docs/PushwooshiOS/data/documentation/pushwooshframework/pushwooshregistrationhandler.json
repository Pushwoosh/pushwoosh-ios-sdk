{"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","interfaceLanguage":"occ"},"abstract":[{"type":"text","text":"Completion handler called when push notification registration completes."}],"metadata":{"modules":[{"name":"PushwooshFramework"}],"roleHeading":"Type Alias","externalID":"c:PWTypes.h@T@PushwooshRegistrationHandler","title":"PushwooshRegistrationHandler","role":"symbol","navigatorTitle":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"fragments":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}],"symbolKind":"typealias"},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"typedef","kind":"keyword"},{"text":" ","kind":"text"},{"preciseIdentifier":"c:BFv(#*$objc(cs)NSString#*$objc(cs)NSError)","kind":"typeIdentifier","text":"void (^)(NSString *, NSError *)"},{"text":" ","kind":"text"},{"kind":"identifier","text":"PushwooshRegistrationHandler"},{"kind":"text","text":";"}]}]},{"kind":"content","content":[{"level":2,"text":"Overview","type":"heading","anchor":"Overview"},{"inlineContent":[{"type":"text","text":"This closure is called when the device registration process finishes, either successfully or with an error."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The handler receives two parameters:"}]},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"token"}]},{"type":"text","text":": The push token string if registration succeeded, or "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if it failed"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"error","type":"text"}],"type":"strong"},{"type":"text","text":": An error object if registration failed, or "},{"code":"nil","type":"codeVoice"},{"text":" if it succeeded","type":"text"}]}]}],"type":"unorderedList"},{"text":"Example","anchor":"Example","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handle registration result with UI updates and backend sync:"}]},{"type":"codeListing","syntax":"swift","code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"]}]}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshframework\/pushwooshregistrationhandler"]}],"sections":[],"seeAlsoSections":[{"title":"Related Documentation","identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler"]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshRegistrationHandler":{"type":"topic","title":"PushwooshRegistrationHandler","kind":"symbol","abstract":[{"text":"Completion handler called when push notification registration completes.","type":"text"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","url":"\/documentation\/pushwooshframework\/pushwooshregistrationhandler","role":"symbol","fragments":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}]},"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework":{"abstract":[{"text":"Push notification SDK for iOS applications.","type":"text"}],"url":"\/documentation\/pushwooshframework","kind":"symbol","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework","title":"PushwooshFramework","type":"topic","role":"collection"},"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshErrorHandler":{"identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler","navigatorTitle":[{"text":"PushwooshErrorHandler","kind":"identifier"}],"type":"topic","fragments":[{"text":"typealias","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"PushwooshErrorHandler"}],"kind":"symbol","role":"symbol","title":"PushwooshErrorHandler","url":"\/documentation\/pushwooshframework\/pushwoosherrorhandler","abstract":[{"text":"Completion handler called when an operation fails.","type":"text"}]}},"variantOverrides":[{"patch":[{"op":"replace","path":"\/references\/doc:~1~1com.pushwoosh.PushwooshiOS~1documentation~1PushwooshFramework~1PushwooshErrorHandler\/title","value":"PushwooshErrorHandler"},{"op":"replace","path":"\/references\/doc:~1~1com.pushwoosh.PushwooshiOS~1documentation~1PushwooshFramework~1PushwooshErrorHandler\/fragments","value":[{"kind":"identifier","text":"PushwooshErrorHandler"}]}],"traits":[{"interfaceLanguage":"occ"}]}]}