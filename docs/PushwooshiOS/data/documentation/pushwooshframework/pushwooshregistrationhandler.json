{","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"],"syntax":"swift"}],"kind":"content"}],"seeAlsoSections":[{"identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler"],"title":"Related Documentation"}],"metadata":{"modules":[{"name":"PushwooshFramework"}],"roleHeading":"Type Alias","externalID":"c:PWTypes.h@T@PushwooshRegistrationHandler","fragments":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"symbolKind":"typealias","title":"PushwooshRegistrationHandler","navigatorTitle":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"role":"symbol"},"schemaVersion":{"major":0,"minor":3,"patch":0},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshframework\/pushwooshregistrationhandler"]}],"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshRegistrationHandler":{"abstract":[{"type":"text","text":"Completion handler called when push notification registration completes."}],"url":"\/documentation\/pushwooshframework\/pushwooshregistrationhandler","type":"topic","identifier":"doc:\/\/com.pushwoo