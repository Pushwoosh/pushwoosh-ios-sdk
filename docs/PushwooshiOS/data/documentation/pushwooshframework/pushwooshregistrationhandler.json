{"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework"]]},"seeAlsoSections":[{"title":"Related Documentation","identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler"]}],"sections":[],"abstract":[{"type":"text","text":"Completion handler called when push notification registration completes."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"typedef"},{"text":" ","kind":"text"},{"kind":"typeIdentifier","text":"void (^)(NSString *, NSError *)","preciseIdentifier":"c:BFv(#*$objc(cs)NSString#*$objc(cs)NSError)"},{"text":" ","kind":"text"},{"text":"PushwooshRegistrationHandler","kind":"identifier"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}]},{"kind":"content","content":[{"type":"heading","level":2,"text":"Overview","anchor":"Overview"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This closure is called when the device registration process finishes, either successfully or with an error."}]},{"inlineContent":[{"text":"The handler receives two parameters:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"token","type":"text"}]},{"type":"text","text":": The push token string if registration succeeded, or "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if it failed"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"error"}],"type":"strong"},{"text":": An error object if registration failed, or ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" if it succeeded","type":"text"}]}]}],"type":"unorderedList"},{"type":"heading","level":2,"text":"Example","anchor":"Example"},{"inlineContent":[{"type":"text","text":"Handle registration result with UI updates and backend sync:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"]}]}],"kind":"symbol","variants":[{"paths":["\/documentation\/pushwooshframework\/pushwooshregistrationhandler"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler"},"metadata":{"modules":[{"name":"PushwooshFramework"}],"externalID":"c:PWTypes.h@T@PushwooshRegistrationHandler","title":"PushwooshRegistrationHandler","symbolKind":"typealias","role":"symbol","navigatorTitle":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"roleHeading":"Type Alias","fragments":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}]},"schemaVersion":{"patch":0,"major":0,"minor":3},"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshErrorHandler":{"kind":"symbol","url":"\/documentation\/pushwooshframework\/pushwoosherrorhandler","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler","type":"topic","abstract":[{"text":"Completion handler called when an operation fails.","type":"text"}],"title":"PushwooshErrorHandler","fragments":[{"kind":"keyword","text":"typealias"},{"text":" ","kind":"text"},{"text":"PushwooshErrorHandler","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"PushwooshErrorHandler"}],"role":"symbol"},"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshRegistrationHandler":{"fragments":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"type":"topic","kind":"symbol","url":"\/documentation\/pushwooshframework\/pushwooshregistrationhandler","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","title":"PushwooshRegistrationHandler","abstract":[{"type":"text","text":"Completion handler called when push notification registration completes."}],"role":"symbol"},"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework":{"url":"\/documentation\/pushwooshframework","kind":"symbol","abstract":[{"text":"Push notification SDK for iOS applications.","type":"text"}],"type":"topic","role":"collection","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework","title":"PushwooshFramework"}},"variantOverrides":[{"patch":[{"value":"PushwooshErrorHandler","path":"\/references\/doc:~1~1com.pushwoosh.PushwooshiOS~1documentation~1PushwooshFramework~1PushwooshErrorHandler\/title","op":"replace"},{"path":"\/references\/doc:~1~1com.pushwoosh.PushwooshiOS~1documentation~1PushwooshFramework~1PushwooshErrorHandler\/fragments","value":[{"text":"PushwooshErrorHandler","kind":"identifier"}],"op":"replace"}],"traits":[{"interfaceLanguage":"occ"}]}]}