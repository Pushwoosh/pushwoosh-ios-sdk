{"abstract":[{"type":"text","text":"Completion handler called when push notification registration completes."}],"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","interfaceLanguage":"occ"},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"text":"typedef","kind":"keyword"},{"kind":"text","text":" "},{"text":"void (^)(NSString *, NSError *)","preciseIdentifier":"c:BFv(#*$objc(cs)NSString#*$objc(cs)NSError)","kind":"typeIdentifier"},{"text":" ","kind":"text"},{"text":"PushwooshRegistrationHandler","kind":"identifier"},{"text":";","kind":"text"}],"languages":["occ"]}],"kind":"declarations"},{"content":[{"type":"heading","text":"Overview","level":2,"anchor":"Overview"},{"type":"paragraph","inlineContent":[{"text":"This closure is called when the device registration process finishes, either successfully or with an error.","type":"text"}]},{"inlineContent":[{"type":"text","text":"The handler receives two parameters:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"token","type":"text"}]},{"text":": The push token string if registration succeeded, or ","type":"text"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if it failed"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"error","type":"text"}]},{"type":"text","text":": An error object if registration failed, or "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if it succeeded"}]}]}]},{"level":2,"type":"heading","text":"Example","anchor":"Example"},{"inlineContent":[{"type":"text","text":"Handle registration result with UI updates and backend sync:"}],"type":"paragraph"},{"code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"],"type":"codeListing","syntax":"swift"}],"kind":"content"}],"sections":[],"metadata":{"roleHeading":"Type Alias","fragments":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}],"externalID":"c:PWTypes.h@T@PushwooshRegistrationHandler","symbolKind":"typealias","role":"symbol","modules":[{"name":"PushwooshFramework"}],"title":"PushwooshRegistrationHandler"},"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework"]]},"kind":"symbol","schemaVersion":{"minor":3,"patch":0,"major":0},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshframework\/pushwooshregistrationhandler"]}],"seeAlsoSections":[{"identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler"],"title":"Related Documentation"}],"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework":{"role":"collection","abstract":[{"text":"Push notification SDK for iOS applications.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework","url":"\/documentation\/pushwooshframework","title":"Pushwo