,"    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"],"type":"codeListing"}]}],"variants":[{"paths":["\/documentation\/pushwooshframework\/pushwooshregistrationhandler"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","interfaceLanguage":"occ"},"abstract":[{"type":"text","text":"Completion handler called when push notification registration completes."}],"seeAlsoSections":[{"title":"Related Documentation","identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler"]}],"kind":"symbol","sections":[],"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework":{"type":"topic","abstract":[{"type":"text","text":"Push notification SDK for iOS applications."}],"url":"\/documentation\/pushwooshframework","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework","kind":"symbol","title":"PushwooshFr