ading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handle registration result with UI updates and backend sync:"}]},{"syntax":"swift","code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"],"type":"codeListing"}],"kind":"content"}],"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","interfaceLanguage":"occ"},"metadata":{"modules":[{"name":"PushwooshFramework"}],"fragments":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"symbolKind":"typealias","navigatorTitle":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}],"role":"symbol","title":"PushwooshRegistrationHandler","roleHeading":"Type Alias","externalID":"c:PWTypes.h@T@PushwooshRegistrationHandler"},"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshRegistrationHandler":{"fragments":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}],"type":"topic","abstract":[{"text":"Completion handler called when push notification registration completes.","type":"text"}],"url":"\/documentation\/pushwoosh