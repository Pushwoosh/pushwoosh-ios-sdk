{"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler"},"primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"text":"typedef","kind":"keyword"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"void (^)(NSString *, NSError *)","preciseIdentifier":"c:BFv(#*$objc(cs)NSString#*$objc(cs)NSError)"},{"kind":"text","text":" "},{"kind":"identifier","text":"PushwooshRegistrationHandler"},{"kind":"text","text":";"}],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"content","content":[{"level":2,"anchor":"Overview","type":"heading","text":"Overview"},{"type":"paragraph","inlineContent":[{"text":"This closure is called when the device registration process finishes, either successfully or with an error.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The handler receives two parameters:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"text":"token","type":"text"}]},{"type":"text","text":": The push token string if registration succeeded, or "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if it failed"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"error"}]},{"text":": An error object if registration failed, or ","type":"text"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if it succeeded"}],"type":"paragraph"}]}]},{"text":"Example","anchor":"Example","level":2,"type":"heading"},{"inlineContent":[{"text":"Handle registration result with UI updates and backend sync:","type":"text"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"]}]}],"metadata":{"externalID":"c:PWTypes.h@T@PushwooshRegistrationHandler","navigatorTitle":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"roleHeading":"Type Alias","title":"PushwooshRegistrationHandler","symbolKind":"typealias","modules":[{"name":"PushwooshFramework"}],"role":"symbol","fragments":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}]},"schemaVersion":{"minor":3,"patch":0,"major":0},"sections":[],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshframework\/pushwooshregistrationhandler"]}],"seeAlsoSections":[{"identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler"],"title":"Related Documentation"}],"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework"]]},"kind":"symbol","abstract":[{"text":"Completion handler called when push notification registration completes.","type":"text"}],"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework":{"role":"collection","abstract":[{"text":"Push notification SDK for iOS applications.","type":"text"}],"title":"PushwooshFramework","kind":"symbol","type":"topic","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework","url":"\/docume