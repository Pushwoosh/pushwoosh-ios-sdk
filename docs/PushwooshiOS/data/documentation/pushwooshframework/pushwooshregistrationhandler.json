{"abstract":[{"text":"Completion handler called when push notification registration completes.","type":"text"}],"sections":[],"metadata":{"externalID":"c:PWTypes.h@T@PushwooshRegistrationHandler","navigatorTitle":[{"text":"PushwooshRegistrationHandler","kind":"identifier"}],"roleHeading":"Type Alias","title":"PushwooshRegistrationHandler","fragments":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}],"role":"symbol","symbolKind":"typealias","modules":[{"name":"PushwooshFramework"}]},"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","interfaceLanguage":"occ"},"seeAlsoSections":[{"identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler"],"title":"Related Documentation"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"typedef"},{"kind":"text","text":" "},{"text":"void (^)(NSString *, NSError *)","kind":"typeIdentifier","preciseIdentifier":"c:BFv(#*$objc(cs)NSString#*$objc(cs)NSError)"},{"kind":"text","text":" "},{"kind":"identifier","text":"PushwooshRegistrationHandler"},{"text":";","kind":"text"}]}]},{"content":[{"type":"heading","level":2,"anchor":"Overview","text":"Overview"},{"inlineContent":[{"type":"text","text":"This closure is called when the device registration process finishes, either successfully or with an error."}],"type":"paragraph"},{"inlineContent":[{"text":"The handler receives two parameters:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"inlineContent":[{"type":"text","text":"token"}],"type":"strong"},{"type":"text","text":": The push token string if registration succeeded, or "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if it failed"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"error"}],"type":"strong"},{"type":"text","text":": An error object if registration failed, or "},{"code":"nil","type":"codeVoice"},{"text":" if it succeeded","type":"text"}]}]}]},{"level":2,"text":"Example","anchor":"Example","type":"heading"},{"inlineContent":[{"type":"text","text":"Handle registration result with UI updates and backend sync:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["func registerForNotifications() {","    let registrationHandler: PushwooshRegistrationHandler = { token, error in","        if let error = error {","            self.logger.error(\"Push registration failed: \\(error.localizedDescription)\")","            self.updateNotificationStatus(.failed)","            return","        }","","        guard let token = token else { return }","","        self.logger.info(\"Push token received: \\(token.prefix(20))...\")","        self.updateNotificationStatus(.registered)","","        self.syncPushTokenWithBackend(token)","    }","","    Pushwoosh.configure.registerForPushNotifications(withCompletion: registrationHandler)","}","","func syncPushTokenWithBackend(_ token: String) {","    let hwid = Pushwoosh.configure.getHWID()","    apiClient.post(\"\/devices\/register\", body: [","        \"push_token\": token,","        \"hwid\": hwid,","        \"platform\": \"ios\"","    ])","}"]}],"kind":"content"}],"kind":"symbol","schemaVersion":{"patch":0,"major":0,"minor":3},"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework"]]},"variants":[{"paths":["\/documentation\/pushwooshframework\/pushwooshregistrationhandler"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework":{"role":"collection","kind":"symbol","abstract":[{"type":"text","text":"Push notification SDK for iOS applications."}],"title":"PushwooshFramework","type":"topic","url":"\/documentation\/pushwooshframework","identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework"},"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshErrorHandler":{"identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshErrorHandler","title":"PushwooshErrorHandler","url":"\/documentation\/pushwooshframework\/pushwoosherrorhandler","abstract":[{"type":"text","text":"Completion handler called when an operation fails."}],"kind":"symbol","type":"topic","role":"symbol","fragments":[{"kind":"keyword","text":"typealias"},{"kind":"text","text":" "},{"text":"PushwooshErrorHandler","kind":"identifier"}],"navigatorTitle":[{"text":"PushwooshErrorHandler","kind":"identifier"}]},"doc://com.pushwoosh.PushwooshiOS/documentation/PushwooshFramework/PushwooshRegistrationHandler":{"identifier":"doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PushwooshRegistrationHandler","title":"PushwooshRegistrationHandler","url":"\/documentation\/pushwooshframework\/pushwooshregistrationhandler","abstract":[{"type":"text","text":"Completion handler called when push notification registration completes."}],"kind":"symbol","type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"PushwooshRegistrationHandler"}]}},"variantOverrides":[{"traits":[{"interfaceLanguage":"occ"}],"patch":[{"op":"replace","value":"PushwooshErrorHandler","path":"\/references\/doc:~1~1com.pushwoosh.PushwooshiOS~1documentation~1PushwooshFramework~1PushwooshErrorHandler\/title"},{"op":"replace","path":"\/references\/doc:~1~1com.pushwoosh.PushwooshiOS~1documentation~1PushwooshFramework~1PushwooshErrorHandler\/fragments","value":[{"text":"PushwooshErrorHandler","kind":"identifier"}]}]}]}