graph","inlineContent":[{"type":"text","text":"This method is invoked when a user explicitly taps on a notification banner or notification in the Notification Center. Use this method to:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Navigate to relevant content"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Open deep links"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"Track user engagement with notifications","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Perform actions based on notification type","type":"text"}]}]}]},{"text":"Timing","level":2,"anchor":"Timing","type":"heading"},{"inlineContent":[{"type":"text","text":"This method is called:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"text","text":"When user taps notification while app is in background"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"When user taps notification while app is terminated (app is launched first)"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"NOT called for silent\/background notifications","type":"text"}]}]}],"type":"unorderedList"},{"type":"heading","level":2,"anchor":"Example","text":"Example"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handle deep links from notification:"}]},{"type":"codeListing","code":["func pushwoosh(_ pushwoosh: Pushwoosh, onMessageOpened message: PWMessage) {","    Analytics.log(\"push_opened\", [","        \"campaign_id\": message.customData?[\"campaign_id\"] ?? \"\"","    ])","","    guard let customData = message.customData else { return }","","    if let deepLink = customData[\"deep_link\"] as? String,","       let url = URL(string: deepLink) {","        DeepLinkRouter.shared.handle(url)","        return","    }","","    if let screen = customData[\"screen\"] as? String {","        navigateToScreen(screen, params: customData)","    }","}","","private func navigateToScreen(_ screen: String, params: [AnyHashable: Any]) {","    switch screen {","    case \"order_details\":","        if let orderId = params[\"order_id\"] as? String {","            router.push(OrderDetailsViewController(orderId: orderId))","        }","    case \"product\":","        if let productId = params[\"product_id\"] as? String {","            router.push(ProductViewController(productId: productId))","        }","    case \"promo\":","        if let promoCode = params[\"promo_code\"] as? String {","            router.push(PromoViewController(code: promoCode))","        }","    default:","        break","    }","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Open specific tab when notification is tapped:"}]},{"code":["func pushwoosh(_ pushwoosh: Pushwoosh, onMessageOpened message: PWMessage) {","    guard let tabIndex = message.customData?[\"tab\"] as? Int else { return }","","    if let tabBarController = window?.rootViewController as? UITabBarController {","        tabBarController.selectedIndex = tabIndex","    }","}"],"syntax":"swift","type":"codeListing"}]}],"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework","doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWMessagingDelegate"]]},"kind":"symbol","schemaVersion":{"patch":0,"minor":3,"major":0},"seeAlsoSections":[{"title":"Related Documentation","identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWMessagingDelegate\/pushwoosh(_:onMessageReceived:)","doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/Pushwoosh\/delegate"]},{"generated":true,"title":"Handling Notifications","identifiers":["doc:\/\/com.pushwoosh.PushwooshiOS\/documentation\/PushwooshFramework\/PWMessagingDelegate\/pushwoosh(_:onMessageReceived:)"]}],"variants":[{"paths":["\/documentation\/pushwooshframework\/pwmessagingdelegate\