{"abstract":[{"type":"text","text":"Processes deep link URLs for Pushwoosh SDK functionality."}],"sections":[],"kind":"symbol","identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/handleOpenURL:","interfaceLanguage":"occ"},"schemaVersion":{"patch":0,"minor":3,"major":0},"variants":[{"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/handleopenurl:"],"traits":[{"interfaceLanguage":"occ"}]}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"+ (","kind":"text"},{"text":"BOOL","kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL"},{"text":") ","kind":"text"},{"text":"handleOpenURL:","kind":"identifier"},{"kind":"text","text":"("},{"text":"NSURL","preciseIdentifier":"c:objc(cs)NSURL","kind":"typeIdentifier"},{"kind":"text","text":" *) "},{"text":"url","kind":"internalParam"},{"text":";","kind":"text"}]}]},{"kind":"parameters","parameters":[{"name":"url","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The URL to process."}]}]}]},{"kind":"content","content":[{"level":2,"text":"Return Value","type":"heading","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"YES if the URL was handled by Pushwoosh, NO otherwise."}]}]},{"content":[{"type":"heading","text":"Discussion","anchor":"discussion","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handles special Pushwoosh URLs, primarily used for:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Test device registration"}]},{"type":"text","text":": Register device for push testing via QR code"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Deep link tracking","type":"text"}]},{"text":": Process tracked deep links from push notifications","type":"text"}]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Should be called from your appâ€™s URL handling methods. Returns YES if the URL"},{"text":" ","type":"text"},{"type":"text","text":"was a Pushwoosh URL and was handled, NO if it should be processed by your app."}]},{"type":"heading","anchor":"Example","level":2,"text":"Example"},{"inlineContent":[{"type":"text","text":"Handle URLs in SceneDelegate (iOS 13+):"}],"type":"paragraph"},{"code":["class SceneDelegate: UIResponder, UIWindowSceneDelegate {","","    func scene(_ scene: UIScene, openURLContexts URLContexts: Set<UIOpenURLContext>) {","        guard let url = URLContexts.first?.url else { return }","","        \/\/ First, check if Pushwoosh handles this URL","        if Pushwoosh.configure.handleOpenURL(url) {","            Logger.info(\"URL handled by Pushwoosh\")","            return","        }","","        \/\/ Handle other deep links","        DeepLinkRouter.shared.handle(url)","    }","","    func scene(_ scene: UIScene, continue userActivity: NSUserActivity) {","        guard userActivity.activityType == NSUserActivityTypeBrowsingWeb,","              let url = userActivity.webpageURL else { return }","","        if Pushwoosh.configure.handleOpenURL(url) {","            return","        }","","        UniversalLinkRouter.shared.handle(url)","    }","}","","\/\/ Legacy AppDelegate support","class AppDelegate: UIApplicationDelegate {","","    func application(_ app: UIApplication,","                    open url: URL,","                    options: [UIApplication.OpenURLOptionsKey: Any] = [:]) -> Bool {","","        if Pushwoosh.configure.handleOpenURL(url) {","            return true","        }","","        return DeepLinkRouter.shared.handle(url)","    }","}"],"type":"codeListing","syntax":"swift"}],"kind":"content"}],"metadata":{"roleHeading":"Type Method","externalID":"c:objc(pl)PWConfiguration(cm)handleOpenURL:","fragments":[{"text":"+ ","kind":"text"},{"text":"handleOpenURL:","kind":"identifier"}],"navigatorTitle":