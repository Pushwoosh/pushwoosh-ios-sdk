{"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/setuserid:completion:"]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setUserId:completion:","interfaceLanguage":"occ"},"sections":[],"kind":"symbol","metadata":{"roleHeading":"Type Method","role":"symbol","modules":[{"name":"PushwooshCore"}],"title":"setUserId:completion:","externalID":"c:objc(pl)PWConfiguration(cm)setUserId:completion:","fragments":[{"kind":"text","text":"+ "},{"text":"setUserId:completion:","kind":"identifier"}],"navigatorTitle":[{"text":"setUserId:completion:","kind":"identifier"}],"symbolKind":"method"},"abstract":[{"type":"text","text":"Sets a custom user identifier with completion confirmation."}],"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"+ ("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":") ","kind":"text"},{"text":"setUserId:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)NSString","text":"NSString","kind":"typeIdentifier"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"userId"},{"text":" ","kind":"text"},{"kind":"identifier","text":"completion:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":" (^","kind":"text"},{"text":")(","kind":"text"},{"preciseIdentifier":"c:objc(cs)NSError","text":"NSError","kind":"typeIdentifier"},{"kind":"text","text":" * "},{"kind":"internalParam","text":"error"},{"kind":"text","text":")) "},{"kind":"internalParam","text":"completion"},{"kind":"text","text":";"}],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The custom user identifier (e.g., database ID, UUID, username)."}]}],"name":"userId"},{"content":[{"inlineContent":[{"type":"text","text":"Block called when the operation completes."},{"text":" ","type":"text"},{"type":"text","text":"Receives nil on success, or an NSError on failure."}],"type":"paragraph"}],"name":"completion"}],"kind":"parameters"},{"content":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Same as ","type":"text"},{"type":"codeVoice","code":"setUserId:"},{"text":" but provides callback when the server operation completes.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"Use this when you need to ensure the user ID is set before performing"},{"type":"text","text":" "},{"type":"text","text":"dependent operations like merging user data or tracking critical events."}]},{"type":"heading","level":2,"text":"Example","anchor":"Example"},{"inlineContent":[{"text":"Handle login with guaranteed user ID synchronization:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["class LoginCoordinator {","","    func performLogin(credentials: Credentials,","                      completion: @escaping (Result<User, Error>) -> Void) {","","        APIClient.shared.login(credentials) { result in","            switch result {","            case .success(let user):","                \/\/ Set user ID with confirmation before proceeding","                Pushwoosh.configure.setUserId(user.id) { error in","                    if let error = error {","                        Logger.warning(\"User ID sync failed: \\(error.localizedDescription)\")","                        \/\/ Continue anyway - local state is still valid","                    }","","                    \/\/ Now safe to track login event with correct user attribution","                    Pushwoosh.configure.setTags([","                        \"logged_in\": true,","                        \"last_login\": ISO8601DateFormatter().string(from: Date()),","                        \"login_method\": credentials.method.rawValue","                    ])","","                    \/\/ Set email if available","                    if let email = user.email {","                        Pushwoosh.configure.setEmail(email)","                    }","","                    completion(.success(user))","                }","","            case .failure(let error):","                completion(.failure(error))","            }","        }","    }","}"]}],"kind":"content"}],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/setUserId:completion:":{"navigatorTitle":[{"kind":"identifier","text":"setUserId:completion:"}],"type":"topic","url":"\/documentation\/pushwooshcore\/pwconfiguration\/setuserid:completion:","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setUserId:completion:","abstract":[{"text":"Sets a custom user identifier with completion confirmation.","type":"text"}],"role":"symbol","title":"setUserId:completion:","kind":"symbol","fragments":[{"kind":"text","text":"+ "},{"kind":"identifier","text":"setUserId:completion:"}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"url":"\/documentation\/pushwooshcore\/pwconfiguration","title":"PWConfiguration","abstract":[],"fragments":[{"text":"PWConfiguration","kind":"identifier"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration","role":"symbol"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"title":"PushwooshCore","url":"\/documentation\/pushwooshcore","kind":"symbol","role":"collection","type":"topic","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]}}}