{"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"kind":"symbol","abstract":[{"text":"Sets a custom user identifier with completion confirmation.","type":"text"}],"sections":[],"primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"text":"+ (","kind":"text"},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"text":") ","kind":"text"},{"kind":"identifier","text":"setUserId:"},{"text":"(","kind":"text"},{"text":"NSString","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSString"},{"kind":"text","text":" *) "},{"text":"userId","kind":"internalParam"},{"kind":"text","text":" "},{"text":"completion:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":" (^","kind":"text"},{"text":")(","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSError","text":"NSError"},{"kind":"text","text":" * "},{"text":"error","kind":"internalParam"},{"text":")) ","kind":"text"},{"text":"completion","kind":"internalParam"},{"text":";","kind":"text"}],"platforms":["iOS"]}],"kind":"declarations"},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"The custom user identifier (e.g., database ID, UUID, username)."}],"type":"paragraph"}],"name":"userId"},{"content":[{"inlineContent":[{"text":"Block called when the operation completes.","type":"text"},{"text":" ","type":"text"},{"text":"Receives nil on success, or an NSError on failure.","type":"text"}],"type":"paragraph"}],"name":"completion"}],"kind":"parameters"},{"content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"inlineContent":[{"type":"text","text":"Same as "},{"code":"setUserId:","type":"codeVoice"},{"type":"text","text":" but provides callback when the server operation completes."},{"type":"text","text":" "},{"type":"text","text":"Use this when you need to ensure the user ID is set before performing"},{"text":" ","type":"text"},{"text":"dependent operations like merging user data or tracking critical events.","type":"text"}],"type":"paragraph"},{"level":2,"text":"Example","anchor":"Example","type":"heading"},{"inlineContent":[{"text":"Handle login with guaranteed user ID synchronization:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["class LoginCoordinator {","","    func performLogin(credentials: Credentials,","                      completion: @escaping (Result<User, Error>) -> Void) {","","        APIClient.shared.login(credentials) { result in","            switch result {","            case .success(let user):","                \/\/ Set user ID with confirmation before proceeding","                Pushwoosh.configure.setUserId(user.id) { error in","                    if let error = error {","                        Logger.warning(\"User ID sync failed: \\(error.localizedDescription)\")","                        \/\/ Continue anyway - local state is still valid","                    }","","                    \/\/ Now safe to track login event with correct user attribution","                    Pushwoosh.configure.setTags([","                        \"logged_in\": true,","                        \"last_login\": ISO8601DateFormatter().string(from: Date()),","                        \"login_method\": credentials.method.rawValue","                    ])","","                    \/\/ Set email if available","                    if let email = user.email {","                        Pushwoosh.configure.setEmail(email)","                    }","","                    completion(.success(user))","                }","","            case .failure(let error):","                completion(.failure(error))","            }","        }","    }","}"]}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/setuserid:completion:"]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setUserId:completion:"},"schemaVersion":{"major":0,"patch":0,"minor":3},"metadata":{"role":"symbol","symbolKind":"method","fragments":[{"text":"+ ","kind":"text"},{"text":"setUserId:completion:","kind":"identifier"}],"roleHeading":"Type Method","modules":[{"name":"PushwooshCore"}],"externalID":"c:objc(pl)PWConfiguration(cm)setUserId:completion:","title":"setUserId:completion:","navigatorTitle":[{"text":"setUserId:completion:","kind":"identifier"}]},"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"type":"topic","kind":"symbol","abstract":[],"title":"PWConfiguration","fragments":[{"kind":"identifier","text":"PWConfiguration"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration","role":"symbol","url":"\/documentation\/pushwooshcore\/pwconfiguration"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"role":"collection","url":"\/documentation\/pushwooshcore","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","type":"topic","kind":"symbol","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/setUserId:completion:":{"navigatorTitle":[{"kind":"identifier","text":"setUserId:completion:"}],"role":"symbol","kind":"symbol","type":"topic","url":"\/documentation\/pushwooshcore\/pwconfiguration\/setuserid:completion:","fragments":[{"text":"+ ","kind":"text"},{"kind":"identifier","text":"setUserId:completion:"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setUserId:completion:","title":"setUserId:completion:","abstract":[{"type":"text","text":"Sets a custom user identifier with completion confirm