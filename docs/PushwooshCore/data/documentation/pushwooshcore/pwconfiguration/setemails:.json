{"variants":[{"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/setemails:"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setEmails:"},"metadata":{"externalID":"c:objc(pl)PWConfiguration(cm)setEmails:","roleHeading":"Type Method","fragments":[{"kind":"text","text":"+ "},{"kind":"identifier","text":"setEmails:"}],"role":"symbol","modules":[{"name":"PushwooshCore"}],"symbolKind":"method","navigatorTitle":[{"kind":"identifier","text":"setEmails:"}],"title":"setEmails:"},"kind":"symbol","schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"abstract":[{"type":"text","text":"Registers multiple email addresses associated with the current user."}],"sections":[],"primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"text":"+ (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"setEmails:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)NSArray","text":"NSArray","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"text":"emails","kind":"internalParam"},{"text":";","kind":"text"}],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"emails","content":[{"inlineContent":[{"text":"Array of email address strings. Each must be a valid email format.","type":"text"}],"type":"paragraph"}]}]},{"content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Associates multiple email addresses with the current device for omnichannel"},{"type":"text","text":" "},{"type":"text","text":"email campaigns. This is useful when users have multiple email addresses"},{"type":"text","text":" "},{"text":"(work, personal, etc.) and want to receive communications on all of them.","type":"text"}]},{"inlineContent":[{"type":"text","text":"All provided emails will be linked to the same device and user ID,"},{"type":"text","text":" "},{"text":"enabling unified targeting across all addresses.","type":"text"}],"type":"paragraph"},{"anchor":"Example","text":"Example","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"Register multiple emails from user profile:","type":"text"}]},{"syntax":"swift","code":["class UserProfileManager {","","    func syncEmailsFromProfile(_ profile: UserProfile) {","        var emails: [String] = []","","        if let primary = profile.primaryEmail {","            emails.append(primary)","        }","        if let secondary = profile.secondaryEmail {","            emails.append(secondary)","        }","        if let work = profile.workEmail {","            emails.append(work)","        }","","        guard !emails.isEmpty else { return }","","        Pushwoosh.configure.setEmails(emails)","","        \/\/ Update tags with email count","        Pushwoosh.configure.setTags([","            \"email_count\": emails.count,","            \"has_work_email\": profile.workEmail != nil,","            \"has_multiple_emails\": emails.count > 1","        ])","    }","}"],"type":"codeListing"}],"kind":"content"}],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/setEmails:":{"fragments":[{"kind":"text","text":"+ "},{"kind":"identifier","text":"setEmails:"}],"abstract":[{"text":"Registers multiple email addresses associated with the current user.","type":"text"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setEmails:","kind":"symbol","type":"topic","url":"\/documentation\/pushwooshcore\/pwconfiguration\/setemails:","role":"symbol","navigatorTitle":[{"text":"setEmails:","kind":"identifier"}],"title"