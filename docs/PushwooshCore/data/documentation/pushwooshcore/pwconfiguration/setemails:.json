{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/setemails:"]}],"sections":[],"metadata":{"fragments":[{"text":"+ ","kind":"text"},{"kind":"identifier","text":"setEmails:"}],"externalID":"c:objc(pl)PWConfiguration(cm)setEmails:","roleHeading":"Type Method","modules":[{"name":"PushwooshCore"}],"navigatorTitle":[{"kind":"identifier","text":"setEmails:"}],"role":"symbol","title":"setEmails:","symbolKind":"method"},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setEmails:"},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"abstract":[{"type":"text","text":"Registers multiple email addresses associated with the current user."}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"+ (","kind":"text"},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"setEmails:"},{"text":"(","kind":"text"},{"text":"NSArray","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSArray"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"emails"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"emails","content":[{"inlineContent":[{"text":"Array of email address strings. Each must be a valid email format.","type":"text"}],"type":"paragraph"}]}]},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"inlineContent":[{"text":"Associates multiple email addresses with the current device for omnichannel","type":"text"},{"text":" ","type":"text"},{"text":"email campaigns. This is useful when users have multiple email addresses","type":"text"},{"type":"text","text":" "},{"text":"(work, personal, etc.) and want to receive communications on all of them.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"All provided emails will be linked to the same device and user ID,"},{"type":"text","text":" "},{"text":"enabling unified targeting across all addresses.","type":"text"}]},{"type":"heading","anchor":"Example","text":"Example","level":2},{"type":"paragraph","inlineContent":[{"text":"Register multiple emails from user profile:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["class UserProfileManager {","","    func syncEmailsFromProfile(_ profile: UserProfile) {","        var emails: [String] = []","","        if let primary = profile.primaryEmail {","            emails.append(primary)","        }","        if let secondary = profile.secondaryEmail {","            emails.append(secondary)","        }","        if let work = profile.workEmail {","            emails.append(work)","        }","","        guard !emails.isEmpty else { return }","","        Pushwoosh.configure.setEmails(emails)","","        \/\/ Update tags with email count","        Pushwoosh.configure.setTags([","            \"email_count\": emails.count,","            \"has_work_email\": profile.workEmail != nil,","            \"has_multiple_emails\": emails.count > 1","        ])","    }","}"]}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"role":"collection","url":"\/documentation\/pushwooshcore","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","type":"topic","kind":"symbol","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/setEmails:":{"fragments":[{"text":"+ ","kind":"text"},{"kind":"identifier","text":"setEmails:"}],"abstract":[{"type":"text","text":"Registers multiple email addresses associated with the current user."}],"url":"\/documentation\/pushwooshcore\/pwconfiguration\/setemails:","type":"topic","navigatorTitle":[{"text":"setEmails:","kind":"identifier"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setEmails:","kind":"symbol","role":"symbol","title":"setEmails:"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"type":"topic","kind":"symbol","abstract":[],"title":"PWConfiguration","fragments":[{"kind":"identifier","text":"PWConfiguration"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration","role":"symbol","url":"\/documentation\/pushwooshcore\/pwconfiguration"}}}