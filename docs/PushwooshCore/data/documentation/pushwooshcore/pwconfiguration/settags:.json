graph"},{"level":2,"anchor":"Supported-Tag-Types","text":"Supported Tag Types","type":"heading"},{"header":"row","rows":[[[{"inlineContent":[{"type":"text","text":"Type"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Example","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Description","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"String","type":"text"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"code":"\"city\": \"New York\"","type":"codeVoice"}]}],[{"type":"paragraph","inlineContent":[{"text":"Text values","type":"text"}]}]],[[{"inlineContent":[{"text":"Integer","type":"text"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"code":"\"age\": 25","type":"codeVoice"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Numeric values"}]}]],[[{"inlineContent":[{"type":"text","text":"Boolean"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"\"isPremium\": true"}]}],[{"type":"paragraph","inlineContent":[{"text":"True\/false values","type":"text"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"List","type":"text"}]}],[{"inlineContent":[{"type":"codeVoice","code":"\"interests\": [\"sports\", \"music\"]"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Arrays of strings"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"Date","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"\"lastPurchase\": Date()"}]}],[{"inlineContent":[{"type":"text","text":"NSDate objects"}],"type":"paragraph"}]]],"type":"table"},{"level":2,"anchor":"Example","text":"Example","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Update user profile tags after account changes:"}]},{"code":["class UserProfileManager {","","    func updateUserTags(for user: User) {","        var tags: [String: Any] = [","            \"user_id\": user.id,","            \"email_verified\": user.isEmailVerified,","            \"account_type\": user.accountType.rawValue,","            \"signup_date\": user.createdAt,","            \"app_version\": Bundle.main.appVersion","        ]","","        \/\/ Add subscription info if premium","        if let subscription = user.subscription {","            tags[\"subscription_tier\"] = subscription.tier.rawValue","            tags[\"subscription_expiry\"] = subscription.expiryDate","            tags[\"is_premium\"] = true","        } else {","            tags[\"is_premium\"] = false","        }","","        \/\/ Add user preferences","        if !user.interests.isEmpty {","            tags[\"interests\"] = user.interests","        }","","        \/\/ Add location if available","        if let location = user.location {","            tags[\"country\"] = location.country","            tags[\"city\"] = location.city","            tags[\"timezone\"] = location.timezone","        }","","        Pushwoosh.configure.setTags(tags)","        Logger.debug(\"User tags updated: \\(tags.keys.joined(separator: \", \"))\")","    }","","    func trackPurchase(product: Product, amount: Decimal) {","        Pushwoosh.configure.setTags([","            \"last_purchase_date\": Date(),","            \"last_purchase_amount\": NSDecimalNumber(decimal: amount),","            \"last_purchase_category\": product.category,","            \"total_purchases\": incrementTag(\"total_purchases\")","        ])","    }","}"],"type":"codeListing","syntax":"swift"}]}],"abstract":[{"type":"text","text":"Sets user tags for device segmentation."}],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"role":"collection","url":"\/documentation\/pushwooshcore","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","type":"topic","kind":"symbol","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfi