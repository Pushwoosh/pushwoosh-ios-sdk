ontent":[{"type":"text","text":"Update UI based on registration status"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Track registration analytics"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"Handle errors gracefully with retry logic","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Sync push token with your backend server"}],"type":"paragraph"}]}]},{"text":"Example","type":"heading","anchor":"Example","level":2},{"inlineContent":[{"type":"text","text":"Handle registration in onboarding flow with analytics tracking:"}],"type":"paragraph"},{"type":"codeListing","code":["class OnboardingViewController: UIViewController {","","    func enableNotificationsTapped() {","        showLoadingState()","","        Pushwoosh.configure.registerForPushNotifications { [weak self] token, error in","            DispatchQueue.main.async {","                self?.hideLoadingState()","","                if let error = error {","                    Analytics.track(\"push_registration_failed\", properties: [","                        \"error\": error.localizedDescription","                    ])","                    self?.showRetryOption()","                    return","                }","","                if let token = token {","                    Analytics.track(\"push_registration_success\")","                    self?.syncTokenWithBackend(token)","                    self?.proceedToNextStep()","                } else {","                    Analytics.track(\"push_permission_denied\")","                    self?.showPermissionDeniedInfo()","                }","            }","        }","    }","","    private func syncTokenWithBackend(_ token: String) {","        APIClient.shared.updatePushToken(token) { result in","            if case .failure(let error) = result {","                Logger.error(\"Backend sync failed: \\(error)\")","            }","        }","    }","}"],"syntax":"swift"}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"variants":[{"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/registerforpushnotificationswithcompletion:"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"abstract":[{"type":"text","text":"Registers the device for push notifications with a completion handler."}],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"role":"collection","url":"\/documentation\/pushwooshcore","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","type":"topic","kind":"symbol","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"type":"topic","kind":"symbol","abstract":[],"title":"PWConfiguration","fragments":[{"kind":"identifier","text":"PWConfiguration"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration","role":"symbol","url":"\/documentation\/pushwooshcore\/pwconfiguration"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/registerForPushNotificationsWithCompletion:":{"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/registerForPushNotificationsWithCompletion:","url":"\/documentation\/pushwooshcore\/pwconfiguration\/registerforpushnotificationswithcompletion:","role":"symbol","fragments":[{"kind":"text","text":"+ "},{"text":"registerForPushNotificationsWithCompletion:","kind":"identifier"}],"kind":"symbol","navigatorTitle":[{"text":"registerForPushNotificationsWithCompletion:","kind":"identifier"}],"title":"registerForPushNotificationsWithCompletion:","type":"topic","abstract":[{"type":"text","text":"Registers the device for pu