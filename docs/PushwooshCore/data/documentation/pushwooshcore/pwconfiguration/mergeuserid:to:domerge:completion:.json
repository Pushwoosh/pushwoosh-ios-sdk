{"kind":"symbol","hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/mergeUserId:to:doMerge:completion:"},"schemaVersion":{"patch":0,"major":0,"minor":3},"sections":[],"metadata":{"navigatorTitle":[{"kind":"identifier","text":"mergeUserId:to:doMerge:completion:"}],"symbolKind":"method","externalID":"c:objc(pl)PWConfiguration(cm)mergeUserId:to:doMerge:completion:","role":"symbol","fragments":[{"text":"+ ","kind":"text"},{"text":"mergeUserId:to:doMerge:completion:","kind":"identifier"}],"modules":[{"name":"PushwooshCore"}],"title":"mergeUserId:to:doMerge:completion:","roleHeading":"Type Method"},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/mergeuserid:to:domerge:completion:"]}],"abstract":[{"text":"Merges engagement data from one user to another.","type":"text"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"+ ("},{"text":"void","kind":"typeIdentifier","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"kind":"identifier","text":"mergeUserId:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:objc(cs)NSString","text":"NSString","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"text":"oldUserId","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"to:"},{"text":"(","kind":"text"},{"text":"NSString","preciseIdentifier":"c:objc(cs)NSString","kind":"typeIdentifier"},{"kind":"text","text":" *) "},{"text":"newUserId","kind":"internalParam"},{"kind":"text","text":" "},{"text":"doMerge:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"BOOL","preciseIdentifier":"c:@T@BOOL"},{"text":") ","kind":"text"},{"text":"doMerge","kind":"internalParam"},{"kind":"text","text":" "},{"text":"completion:","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"kind":"text","text":" (^"},{"text":")(","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSError","text":"NSError"},{"text":" * ","kind":"text"},{"kind":"internalParam","text":"error"},{"kind":"text","text":")) "},{"kind":"internalParam","text":"completion"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"parameters":[{"name":"oldUserId","content":[{"type":"paragraph","inlineContent":[{"text":"The source user identifier whose data will be transferred\/deleted.","type":"text"}]}]},{"name":"newUserId","content":[{"inlineContent":[{"type":"text","text":"The destination user identifier to receive the merged data."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"If YES, transfers all events to newUserId. If NO, deletes events for oldUserId."}],"type":"paragraph"}],"name":"doMerge"},{"content":[{"inlineContent":[{"text":"Block called when the operation completes.","type":"text"},{"text":" ","type":"text"},{"text":"Receives nil on success, or an NSError on failure.","type":"text"}],"type":"paragraph"}],"name":"completion"}],"kind":"parameters"},{"content":[{"level":2,"anchor":"discussion","text":"Discussion","type":"heading"},{"inlineContent":[{"text":"Transfers or removes push engagement history when user identity changes.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"Common scenarios include:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"text":"Anonymous to authenticated","type":"text"}]},{"type":"text","text":": Merge anonymous browsing data to logged-in user"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Account linking","type":"text"}]},{"type":"text","text":": Combine data when user links multiple accounts"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Account recovery","type":"text"}]},{"text":": Transfer data to recovered account","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Data cleanup","type":"text"}]},{"text":": Remove data for deleted\/merged accounts","type":"text"}]}]}],"type":"unorderedList"},{"inlineContent":[{"text":"When ","type":"text"},{"type":"codeVoice","code":"doMerge"},{"text":" is YES, all events (push opens, in-app interactions, etc.)","type":"text"},{"text":" ","type":"text"},{"text":"from ","type":"text"},{"code":"oldUserId","type":"codeVoice"},{"type":"text","text":" are moved to "},{"type":"codeVoice","code":"newUserId"},{"type":"text","text":". When NO, events for "},{"type":"codeVoice","code":"oldUserId"},{"text":" ","type":"text"},{"type":"text","text":"are simply deleted."}],"type":"paragraph"},{"type":"heading","anchor":"Example","text":"Example","level":2},{"type":"paragraph","inlineContent":[{"text":"Merge anonymous user data after sign-up:","type":"text"}]},{"syntax":"swift","code":["class UserIdentityManager {","","    private var anonymousUserId: String?","","    func startAnonymousSession() {","        \/\/ Generate anonymous ID for tracking before login","        anonymousUserId = \"anon_\\(UUID().uuidString)\"","        Pushwoosh.configure.setUserId(anonymousUserId!)","","        Pushwoosh.configure.setTags([","            \"is_anonymous\": true,","            \"session_start\": ISO8601DateFormatter().string(from: Date())","        ])","    }","","    func convertToAuthenticatedUser(_ authenticatedUser: User,","                                    completion: @escaping (Result<Void, Error>) -> Void) {","","        guard let anonId = anonymousUserId else {","            \/\/ No anonymous session to merge","            setupAuthenticatedUser(authenticatedUser, completion: completion)","            return","        }","","        \/\/ First set the new authenticated user ID","        Pushwoosh.configure.setUser(authenticatedUser.id, email: authenticatedUser.email) { [weak self] error in","            if let error = error {","                completion(.failure(error))","                return","            }","","            \/\/ Merge anonymous engagement data to authenticated user","            Pushwoosh.configure.mergeUserId(anonId, to: authenticatedUser.id, doMerge: true) { mergeError in","                if let mergeError = mergeError {","                    Logger.warning(\"Merge failed: \\(mergeError.localizedDescription)\")","                    \/\/ Continue anyway - user is authenticated","                }","","                \/\/ Update tags for authenticated state","                Pushwoosh.configure.setTags([","                    \"is_anonymous\": false,","                    \"conversion_date\": ISO8601DateFormatter().string(from: Date()),","                    \"had_anonymous_session\": true","                ])","","                self?.anonymousUserId = nil","                completion(.success(()))","            }","        }","    }","","    func handleAccountDeletion(userId: String, completion: @escaping (Result<Void, Error>) -> Void) {","        \/\/ Remove user data without merging (doMerge: false)","        Pushwoosh.configure.mergeUserId(userId, to: \"\", doMerge: false) { error in","            if let error = error {","                completion(.failure(error))","                return","            }","","            Pushwoosh.configure.setUserId(\"\")","            Pushwoosh.configure.stopServerCommunication()","            completion(.success(()))","        }","    }","}"],"type":"codeListing"}],"kind":"content"}],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"url":"\/documentation\/pushwooshcore\/pwconfiguration","title":"PWConfiguration","abstract":[],"fragments":[{"text":"PWConfiguration","kind":"identifier"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration","role":"symbol"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"title":"PushwooshCore","url":"\/documentation\/pushwooshcore","kind":"symbol","role":"collection","type":"topic","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/mergeUserId:to:doMerge:completion:":{"navigatorTitle":[{"text":"mergeUserId:to:doMerge:completion:","kind":"identifier"}],"type":"topic","url":"\/documentation\/pushwooshcore\/pwconfiguration\/mergeuserid:to:domerge:completion:","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/mergeUserId:to:doMerge:completion:","abstract":[{"text":"Merges engagement data from one user to another.","type":"text"}],"role":"symbol","title":"mergeUserId:to:doMerge:completion:","kind":"symbol","fragments":[{"text":"+ ","kind":"text"},{"kind":"identifier","text":"mergeUserId:to:doMerge:completion:"}]}}}