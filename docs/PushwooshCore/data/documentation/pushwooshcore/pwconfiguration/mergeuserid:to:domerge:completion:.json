{"metadata":{"role":"symbol","roleHeading":"Type Method","modules":[{"name":"PushwooshCore"}],"title":"mergeUserId:to:doMerge:completion:","symbolKind":"method","externalID":"c:objc(pl)PWConfiguration(cm)mergeUserId:to:doMerge:completion:","navigatorTitle":[{"kind":"identifier","text":"mergeUserId:to:doMerge:completion:"}],"fragments":[{"kind":"text","text":"+ "},{"text":"mergeUserId:to:doMerge:completion:","kind":"identifier"}]},"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/mergeUserId:to:doMerge:completion:"},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"+ (","kind":"text"},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"mergeUserId:"},{"text":"(","kind":"text"},{"text":"NSString","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSString"},{"kind":"text","text":" *) "},{"text":"oldUserId","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"to:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"NSString","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSString"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"newUserId"},{"kind":"text","text":" "},{"kind":"identifier","text":"doMerge:"},{"text":"(","kind":"text"},{"text":"BOOL","kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL"},{"kind":"text","text":") "},{"text":"doMerge","kind":"internalParam"},{"kind":"text","text":" "},{"kind":"identifier","text":"completion:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":" (^","kind":"text"},{"text":")(","kind":"text"},{"text":"NSError","preciseIdentifier":"c:objc(cs)NSError","kind":"typeIdentifier"},{"text":" * ","kind":"text"},{"kind":"internalParam","text":"error"},{"kind":"text","text":")) "},{"text":"completion","kind":"internalParam"},{"text":";","kind":"text"}],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"name":"oldUserId","content":[{"inlineContent":[{"text":"The source user identifier whose data will be transferred\/deleted.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"The destination user identifier to receive the merged data.","type":"text"}]}],"name":"newUserId"},{"name":"doMerge","content":[{"inlineContent":[{"type":"text","text":"If YES, transfers all events to newUserId. If NO, deletes events for oldUserId."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Block called when the operation completes."},{"text":" ","type":"text"},{"text":"Receives nil on success, or an NSError on failure.","type":"text"}]}],"name":"completion"}]},{"content":[{"level":2,"anchor":"discussion","type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Transfers or removes push engagement history when user identity changes."},{"text":" ","type":"text"},{"text":"Common scenarios include:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"inlineContent":[{"type":"text","text":"Anonymous to authenticated"}],"type":"strong"},{"type":"text","text":": Merge anonymous browsing data to logged-in user"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Account linking"}]},{"type":"text","text":": Combine data when user links multiple accounts"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"Account recovery"}],"type":"strong"},{"type":"text","text":": Transfer data to recovered account"}]}]},{"content":[{"inlineContent":[{"inlineContent":[{"text":"Data cleanup","type":"text"}],"type":"strong"},{"type":"text","text":": Remove data for deleted\/merged accounts"}],"type":"paragraph"}]}]},{"inlineContent":[{"text":"When ","type":"text"},{"type":"codeVoice","code":"doMerge"},{"type":"text","text":" is YES, all events (push opens, in-app interactions, etc.)"},{"type":"text","text":" "},{"text":"from ","type":"text"},{"type":"codeVoice","code":"oldUserId"},{"type":"text","text":" are moved to "},{"type":"codeVoice","code":"newUserId"},{"text":". When NO, events for ","type":"text"},{"code":"oldUserId","type":"codeVoice"},{"text":" ","type":"text"},{"text":"are simply deleted.","type":"text"}],"type":"paragraph"},{"anchor":"Example","type":"heading","level":2,"text":"Example"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Merge anonymous user data after sign-up:"}]},{"type":"codeListing","syntax":"swift","code":["class UserIdentityManager {","","    private var anonymousUserId: String?","","    func startAnonymousSession() {","        \/\/ Generate anonymous ID for tracking before login","        anonymousUserId = \"anon_\\(UUID().uuidString)\"","        Pushwoosh.configure.setUserId(anonymousUserId!)","","        Pushwoosh.configure.setTags([","            \"is_anonymous\": true,","            \"session_start\": ISO8601DateFormatter().string(from: Date())","        ])","    }","","    func convertToAuthenticatedUser(_ authenticatedUser: User,","                                    completion: @escaping (Result<Void, Error>) -> Void) {","","        guard let anonId = anonymousUserId else {","            \/\/ No anonymous session to merge","            setupAuthenticatedUser(authenticatedUser, completion: completion)","            return","        }","","        \/\/ First set the new authenticated user ID","        Pushwoosh.configure.setUser(authenticatedUser.id, email: authenticatedUser.email) { [weak self] error in","            if let error = error {","                completion(.failure(error))","                return","            }","","            \/\/ Merge anonymous engagement data to authenticated user","            Pushwoosh.configure.mergeUserId(anonId, to: authenticatedUser.id, doMerge: true) { mergeError in","                if let mergeError = mergeError {","                    Logger.warning(\"Merge failed: \\(mergeError.localizedDescription)\")","                    \/\/ Continue anyway - user is authenticated","                }","","                \/\/ Update tags for authenticated state","                Pushwoosh.configure.setTags([","                    \"is_anonymous\": false,","                    \"conversion_date\": ISO8601DateFormatter().string(from: Date()),","                    \"had_anonymous_session\": true","                ])","","                self?.anonymousUserId = nil","                completion(.success(()))","            }","        }","    }","","    func handleAccountDeletion(userId: String, completion: @escaping (Result<Void, Error>) -> Void) {","        \/\/ Remove user data without merging (doMerge: false)","        Pushwoosh.configure.mergeUserId(userId, to: \"\", doMerge: false) { error in","            if let error = error {","                completion(.failure(error))","                return","            }","","            Pushwoosh.configure.setUserId(\"\")","            Pushwoosh.configure.stopServerCommunication()","            completion(.success(()))","        }","    }","}"]}],"kind":"content"}],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"abstract":[{"text":"Merges engagement data from one user to another.","type":"text"}],"sections":[],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/mergeuserid:to:domerge:completion:"]}],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"role":"collection","url":"\/documentation\/pushwooshcore","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","type":"topic","kind":"symbol","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"type":"topic","kind":"symbol","abstract":[],"title":"PWConfiguration","fragments":[{"kind":"identifier","text":"PWConfiguration"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration","role":"symbol","url":"\/documentation\/pushwooshcore\/pwconfiguration"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/mergeUserId:to:doMerge:completion:":{"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/mergeUserId:to:doMerge:completion:","url":"\/documentation\/pushwooshcore\/pwconfiguration\/mergeuserid:to:domerge:completion:","role":"symbol","fragments":[{"kind":"text","text":"+ "},{"text":"mergeUserId:to:doMerge:completion:","kind":"identifier"}],"kind":"symbol","navigatorTitle":[{"text":"mergeUserId:to:doMerge:completion:","kind":"identifier"}],"type":"topic","title":"mergeUserId:to:doMerge:completion:","abstract":[{"type":"text","text":"Merges engagement data from one user to another."}]}}}