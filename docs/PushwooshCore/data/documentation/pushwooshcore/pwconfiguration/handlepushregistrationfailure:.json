{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/handlepushregistrationfailure:"]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/handlePushRegistrationFailure:"},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"abstract":[{"text":"Handles push notification registration failure.","type":"text"}],"metadata":{"modules":[{"name":"PushwooshCore"}],"externalID":"c:objc(pl)PWConfiguration(cm)handlePushRegistrationFailure:","title":"handlePushRegistrationFailure:","fragments":[{"text":"+ ","kind":"text"},{"text":"handlePushRegistrationFailure:","kind":"identifier"}],"symbolKind":"method","navigatorTitle":[{"text":"handlePushRegistrationFailure:","kind":"identifier"}],"role":"symbol","roleHeading":"Type Method"},"schemaVersion":{"patch":0,"major":0,"minor":3},"primaryContentSections":[{"declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"text":"+ (","kind":"text"},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":") ","kind":"text"},{"text":"handlePushRegistrationFailure:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)NSError","kind":"typeIdentifier","text":"NSError"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"error"},{"kind":"text","text":";"}]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The NSError received from APNs registration failure","type":"text"}]}],"name":"error"}]},{"content":[{"level":2,"anchor":"discussion","type":"heading","text":"Discussion"},{"inlineContent":[{"type":"text","text":"Call this method from your AppDelegateâ€™s "},{"type":"codeVoice","code":"application:didFailToRegisterForRemoteNotificationsWithError:"},{"type":"text","text":" "},{"type":"text","text":"to notify Pushwoosh about registration failures."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"This allows Pushwoosh to:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"text","text":"Log the error for debugging"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Retry registration if appropriate"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Update device status in the Control Panel"}],"type":"paragraph"}]}]},{"level":2,"type":"heading","text":"Example","anchor":"Example"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handle registration failure with user feedback:"}]},{"syntax":"swift","type":"codeListing","code":["class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                    didFailToRegisterForRemoteNotificationsWithError error: Error) {","","        \/\/ Forward to Pushwoosh","        Pushwoosh.configure.handlePushRegistrationFailure(error as NSError)","","        \/\/ Log for debugging","        Logger.error(\"Push registration failed: \\(error.localizedDescription)\")","","        \/\/ Handle specific error cases","        let nsError = error as NSError","        switch nsError.code {","        case 3010:","            \/\/ Simulator or push not supported","            Logger.warning(\"Push notifications not supported on this device\")","","        default:","            \/\/ Track error in analytics","            Analytics.track(\"push_registration_failed\", properties: [","                \"error_code\": nsError.code,","                \"error_message\": nsError.localizedDescription","            ])","","            \/\/ Optionally notify user","            if shouldShowPushError {","                showPushRegistrationError()","            }","        }","    }","}"]}],"kind":"content"}],"sections":[],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"kind":"symbol","title":"PWConfiguration","role":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"PWConfiguration"}],"url":"\/documentation\/pushwooshcore\/pwconfiguration","abstract":[],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"type":"topic","role":"collection","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}],"url":"\/documentation\/pushwooshcore","kind":"symbol","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/handlePushRegistrationFailure:":{"url":"\/documentation\/pushwooshcore\/pwconfiguration\/handlepushregistrationfailure:","abstract":[{"type":"text","text":"Handles push notification registration failure."}],"kind":"symbol","title":"handlePushRegistrationFailure:","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/handlePushRegistrationFailure:","navigatorTitle":[{"text":"handlePushRegistrationFailure:","kind":"identifier"}],"role":"symbol","fragments":[{"text":"+ ","kind":"text"},{"text":"handlePushRegistrationFailure:","kind":"identifier"}],"type":"topic"}}}