{"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/handlePushRegistrationFailure:","interfaceLanguage":"occ"},"kind":"symbol","abstract":[{"type":"text","text":"Handles push notification registration failure."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"+ ("},{"text":"void","kind":"typeIdentifier","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"kind":"identifier","text":"handlePushRegistrationFailure:"},{"kind":"text","text":"("},{"text":"NSError","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSError"},{"kind":"text","text":" *) "},{"text":"error","kind":"internalParam"},{"text":";","kind":"text"}],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"name":"error","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The NSError received from APNs registration failure"}]}]}]},{"kind":"content","content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Call this method from your AppDelegateâ€™s "},{"type":"codeVoice","code":"application:didFailToRegisterForRemoteNotificationsWithError:"},{"type":"text","text":" "},{"type":"text","text":"to notify Pushwoosh about registration failures."}]},{"type":"paragraph","inlineContent":[{"text":"This allows Pushwoosh to:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Log the error for debugging"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Retry registration if appropriate"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Update device status in the Control Panel"}],"type":"paragraph"}]}]},{"level":2,"type":"heading","text":"Example","anchor":"Example"},{"inlineContent":[{"text":"Handle registration failure with user feedback:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                    didFailToRegisterForRemoteNotificationsWithError error: Error) {","","        \/\/ Forward to Pushwoosh","        Pushwoosh.configure.handlePushRegistrationFailure(error as NSError)","","        \/\/ Log for debugging","        Logger.error(\"Push registration failed: \\(error.localizedDescription)\")","","        \/\/ Handle specific error cases","        let nsError = error as NSError","        switch nsError.code {","        case 3010:","            \/\/ Simulator or push not supported","            Logger.warning(\"Push notifications not supported on this device\")","","        default:","            \/\/ Track error in analytics","            Analytics.track(\"push_registration_failed\", properties: [","                \"error_code\": nsError.code,","                \"error_message\": nsError.localizedDescription","            ])","","            \/\/ Optionally notify user","            if shouldShowPushError {","                showPushRegistrationError()","            }","        }","    }","}"]}]}],"variants":[{"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/handlepushregistrationfailure:"],"traits":[{"interfaceLanguage":"occ"}]}],"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"sections":[],"metadata":{"symbolKind":"method","modules":[{"name":"PushwooshCore"}],"externalID":"c:objc(pl)PWConfiguration(cm)handlePushRegistrationFailure:","navigatorTitle":[{"text":"handlePushRegistrationFailure:","kind":"identifier"}],"role":"symbol","title":"handlePushRegistrationFailure:","roleHeading":"Type Method","fragments":[{"text":"+ ","kind":"text"},{"text":"handlePushRegistrationFailure:","kind":"iden