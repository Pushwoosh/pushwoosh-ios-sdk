{"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"schemaVersion":{"patch":0,"major":0,"minor":3},"sections":[],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"+ ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"handlePushRegistrationFailure:"},{"text":"(","kind":"text"},{"text":"NSError","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSError"},{"kind":"text","text":" *) "},{"text":"error","kind":"internalParam"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"name":"error","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The NSError received from APNs registration failure"}]}]}]},{"kind":"content","content":[{"type":"heading","text":"Discussion","anchor":"discussion","level":2},{"inlineContent":[{"text":"Call this method from your AppDelegateâ€™s ","type":"text"},{"type":"codeVoice","code":"application:didFailToRegisterForRemoteNotificationsWithError:"},{"text":" ","type":"text"},{"type":"text","text":"to notify Pushwoosh about registration failures."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"This allows Pushwoosh to:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Log the error for debugging"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Retry registration if appropriate"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Update device status in the Control Panel"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"heading","anchor":"Example","level":2,"text":"Example"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Handle registration failure with user feedback:"}]},{"syntax":"swift","code":["class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                    didFailToRegisterForRemoteNotificationsWithError error: Error) {","","        \/\/ Forward to Pushwoosh","        Pushwoosh.configure.handlePushRegistrationFailure(error as NSError)","","        \/\/ Log for debugging","        Logger.error(\"Push registration failed: \\(error.localizedDescription)\")","","        \/\/ Handle specific error cases","        let nsError = error as NSError","        switch nsError.code {","        case 3010:","            \/\/ Simulator or push not supported","            Logger.warning(\"Push notifications not supported on this device\")","","        default:","            \/\/ Track error in analytics","            Analytics.track(\"push_registration_failed\", properties: [","                \"error_code\": nsError.code,","                \"error_message\": nsError.localizedDescription","            ])","","            \/\/ Optionally notify user","            if shouldShowPushError {","                showPushRegistrationError()","            }","        }","    }","}"],"type":"codeListing"}]}],"kind":"symbol","metadata":{"role":"symbol","externalID":"c:objc(pl)PWConfiguration(cm)handlePushRegistrationFailure:","fragments":[{"text":"+ ","kind":"text"},{"text":"handlePushRegistrationFailure:","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"handlePushRegistrationFailure:"}],"roleHeading":"Type Method","symbolKind":"method","title":"handlePushRegistrationFailure:","modules":[{"name":"PushwooshCore"}]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/handlePushRegistrationFailure:"},"abstract":[{"type":"text","text":"Handles push notification registration failure."}],"variants":[{"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/handlepushregistrationfailure:"],"traits":[{"interfaceLanguage"