{"schemaVersion":{"major":0,"patch":0,"minor":3},"variants":[{"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/setuser:emails:"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"navigatorTitle":[{"text":"setUser:emails:","kind":"identifier"}],"roleHeading":"Type Method","symbolKind":"method","externalID":"c:objc(pl)PWConfiguration(cm)setUser:emails:","title":"setUser:emails:","fragments":[{"text":"+ ","kind":"text"},{"kind":"identifier","text":"setUser:emails:"}],"role":"symbol","modules":[{"name":"PushwooshCore"}]},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"+ ("},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"kind":"text","text":") "},{"kind":"identifier","text":"setUser:"},{"text":"(","kind":"text"},{"text":"NSString","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSString"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"userId"},{"kind":"text","text":" "},{"text":"emails:","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"NSArray","preciseIdentifier":"c:objc(cs)NSArray"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"emails"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The custom user identifier.","type":"text"}]}],"name":"userId"},{"name":"emails","content":[{"inlineContent":[{"type":"text","text":"Array of email address strings to associate with the user."}],"type":"paragraph"}]}]},{"content":[{"text":"Discussion","anchor":"discussion","level":2,"type":"heading"},{"inlineContent":[{"type":"text","text":"Convenience method that combines "},{"code":"setUserId:","type":"codeVoice"},{"text":" and ","type":"text"},{"code":"setEmails:","type":"codeVoice"},{"type":"text","text":" into a single"},{"type":"text","text":" "},{"text":"server request. This is more efficient than calling both methods separately","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"and ensures atomic association of user ID with emails."}],"type":"paragraph"},{"inlineContent":[{"text":"Use this during login or registration flows when you have both user ID","type":"text"},{"type":"text","text":" "},{"type":"text","text":"and email available simultaneously."}],"type":"paragraph"},{"level":2,"type":"heading","text":"Example","anchor":"Example"},{"inlineContent":[{"text":"Complete registration with user ID and emails:","type":"text"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["class RegistrationManager {","","    func completeRegistration(_ registration: RegistrationData) {","        \/\/ Create user on backend","        APIClient.shared.createUser(registration) { result in","            guard case .success(let user) = result else { return }","","            \/\/ Set both user ID and emails in single operation","            var emails = [user.email]","            if let secondaryEmail = registration.secondaryEmail {","                emails.append(secondaryEmail)","            }","","            Pushwoosh.configure.setUser(user.id, emails: emails)","","            \/\/ Set initial user tags","            Pushwoosh.configure.setTags([","                \"registration_date\": ISO8601DateFormatter().string(from: Date()),","                \"registration_source\": registration.source.rawValue,","                \"account_type\": user.accountType.rawValue,","                \"has_completed_onboarding\": false","            ])","        }","    }","}"]},{"type":"codeListing","syntax":null,"code":["   when you need confirmation."]}],"kind":"content"}],"sections":[],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"abstract":[{"type":"text","text":"Sets user identifier and emails together in a single operation."}],"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setUser:emails:","interfaceLanguage":"occ"},"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"kind":"symbol","title":"PWConfiguration","role":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"PWConfiguration"}],"url":"\/documentation\/pushwooshcore\/pwconfiguration","abstract":[],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/setUser:emails:":{"url":"\/documentation\/pushwooshcore\/pwconfiguration\/setuser:emails:","abstract":[{"type":"text","text":"Sets user identifier and emails together in a single operation."}],"kind":"symbol","title":"setUser:emails:","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/setUser:emails:","navigatorTitle":[{"text":"setUser:emails:","kind":"identifier"}],"role":"symbol","fragments":[{"text":"+ ","kind":"text"},{"text":"setUser:emails:","kind":"identifier"}],"type":"topic"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"type":"topic","role":"collection","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}],"url":"\/documentation\/pushwooshcore","kind":"symbol","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore"}}}