{"abstract":[{"text":"Unregisters the device from push notifications.","type":"text"}],"metadata":{"modules":[{"name":"PushwooshCore"}],"title":"unregisterForPushNotifications:","fragments":[{"kind":"text","text":"+ "},{"text":"unregisterForPushNotifications:","kind":"identifier"}],"roleHeading":"Type Method","externalID":"c:objc(pl)PWConfiguration(cm)unregisterForPushNotifications:","role":"symbol","navigatorTitle":[{"text":"unregisterForPushNotifications:","kind":"identifier"}],"symbolKind":"method"},"schemaVersion":{"minor":3,"major":0,"patch":0},"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/unregisterForPushNotifications:"},"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"+ ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"text":") ","kind":"text"},{"kind":"identifier","text":"unregisterForPushNotifications:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":" (^","kind":"text"},{"text":")(","kind":"text"},{"preciseIdentifier":"c:objc(cs)NSError","kind":"typeIdentifier","text":"NSError"},{"text":" * ","kind":"text"},{"text":"error","kind":"internalParam"},{"kind":"text","text":")) "},{"kind":"internalParam","text":"completion"},{"kind":"text","text":";"}],"languages":["occ"],"platforms":["iOS"]}]},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"Block called when unregistration completes.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Receives nil on success or an NSError on failure."}]}],"name":"completion"}],"kind":"parameters"},{"content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"inlineContent":[{"type":"text","text":"Removes the device from receiving push notifications:"}],"type":"paragraph"},{"type":"orderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"Notifies Pushwoosh servers to stop sending notifications to this device","type":"text"}]}]},{"content":[{"inlineContent":[{"text":"Removes the device token from Pushwoosh","type":"text"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"text":"Use this when the user logs out, deletes their account, or explicitly disables notifications.","type":"text"}]},{"type":"heading","level":2,"anchor":"Example","text":"Example"},{"type":"paragraph","inlineContent":[{"text":"Unregister during user logout:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["class AuthenticationManager {","","    func logout(completion: @escaping (Result<Void, Error>) -> Void) {","        \/\/ Show loading indicator","        LoadingIndicator.show()","","        \/\/ First, unregister from push to stop receiving notifications for this user","        Pushwoosh.configure.unregisterForPushNotifications { [weak self] error in","            if let error = error {","                Logger.warning(\"Push unregistration failed: \\(error.localizedDescription)\")","                \/\/ Continue with logout even if unregistration fails","            }","","            \/\/ Clear user session","            self?.clearUserSession()","","            \/\/ Clear user tags","            Pushwoosh.configure.setTags([","                \"logged_in\": false,","                \"user_id\": NSNull(),","                \"email\": NSNull()","            ])","","            \/\/ Stop server communication if required by privacy policy","            if UserDefaults.standard.bool(forKey: \"clearDataOnLogout\") {","                Pushwoosh.configure.stopServerCommunication()","            }","","            LoadingIndicator.hide()","            completion(.success(()))","        }","    }","","    private func clearUserSession() {","        KeychainManager.shared.clearCredentials()","        UserDefaults.standard.removePersistentDomain(forName: Bundle.main.bundleIdentifier!)","        CoreDataManager.shared.clearUserData()","    }","}"]}],"kind":"content"}],"variants":[{"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/unregisterforpushnotifications:"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore":{"role":"collection","url":"\/documentation\/pushwooshcore","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","type":"topic","kind":"symbol","title":"PushwooshCore","abstract":[{"type":"text","text":"Core foundation module for Pushwoosh SDK."}]},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/unregisterForPushNotifications:":{"title":"unregisterForPushNotifications:","navigatorTitle":[{"text":"unregisterForPushNotifications:","kind":"identifier"}],"url":"\/documentation\/pushwooshcore\/pwconfiguration\/unregisterforpushnotifications:","kind":"symbol","fragments":[{"text":"+ ","kind":"text"},{"text":"unregisterForPushNotifications:","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/unregisterForPushNotifications:","abstract":[{"type":"text","text":"Unregisters the device from push notifications."}],"type":"topic"},"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration":{"type":"topic","kind":"symbol","abstract":[],"title":"PWConfiguration","fragments":[{"kind":"identifier","text":"PWConfiguration"}],"identifier":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration","role":"symbol","url":"\/documentation\/pushwooshcore\/pwconfiguration"}}}