{"sections":[],"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"fragments":[{"text":"+ ","kind":"text"},{"text":"getHWID","kind":"identifier"}],"modules":[{"name":"PushwooshCore"}],"navigatorTitle":[{"text":"getHWID","kind":"identifier"}],"externalID":"c:objc(pl)PWConfiguration(cm)getHWID","title":"getHWID","symbolKind":"method","roleHeading":"Type Method","role":"symbol"},"kind":"symbol","primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"kind":"text","text":"+ ("},{"kind":"typeIdentifier","text":"NSString","preciseIdentifier":"c:objc(cs)NSString"},{"text":" *) ","kind":"text"},{"kind":"identifier","text":"getHWID"},{"text":";","kind":"text"}],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"content","content":[{"anchor":"return-value","level":2,"text":"Return Value","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"The unique Pushwoosh device identifier (never nil)","type":"text"}]}]},{"content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"inlineContent":[{"text":"The HWID is a unique device identifier used in all Pushwoosh API calls and for device tracking","type":"text"},{"text":" ","type":"text"},{"text":"in the Pushwoosh Control Panel. On iOS, this is derived from ","type":"text"},{"code":"UIDevice.identifierForVendor","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"The HWID remains consistent across app launches and is used to:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Identify the device in the Pushwoosh Control Panel"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Track user behavior and engagement"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Target specific devices for push notifications"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Link devices to user accounts in your backend"}]}]}]},{"type":"heading","text":"Example","level":2,"anchor":"Example"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Sync device with your backend for cross-platform user tracking:"}]},{"syntax":"swift","type":"codeListing","code":["class DeviceRegistrationService {","","    func registerDeviceWithBackend(userId: String) {","        let hwid = Pushwoosh.configure.getHWID()","        let pushToken = Pushwoosh.configure.getPushToken()","","        let deviceInfo: [String: Any] = [","            \"hwid\": hwid,","            \"userId\": userId,","            \"pushToken\": pushToken ?? \"\",","            \"platform\": \"ios\",","            \"appVersion\": Bundle.main.appVersion,","            \"osVersion\": UIDevice.current.systemVersion","        ]","","        APIClient.shared.post(\"\/api\/devices\/register\", body: deviceInfo) { result in","            switch result {","            case .success:","                Logger.info(\"Device registered with backend: \\(hwid)\")","            case .failure(let error):","                Logger.error(\"Device registration failed: \\(error)\")","            }","        }","    }","}"]},{"inlineContent":[{"type":"text","text":"It may change if the user reinstalls all apps from the same vendor."}],"type":"paragraph"}],"kind":"content"}],"abstract":[{"text":"Returns the Pushwoosh Hardware ID (HWID) for this device.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshcore\/pwconfiguration\/gethwid"]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration\/getHWID"},"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore","doc:\/\/com.pushwoosh.PushwooshCore\/documentation\/PushwooshCore\/PWConfiguration"]]},"references":{"doc://com.pushwoosh.PushwooshCore/documentation/PushwooshCore/PWConfiguration/getHWID":{"