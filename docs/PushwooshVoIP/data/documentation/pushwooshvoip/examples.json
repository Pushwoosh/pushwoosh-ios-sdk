 true","}","","extension AppDelegate: PWVoIPCallDelegate {","","    func voipDidReceiveIncomingCall(payload: PushwooshVoIP.PWVoIPMessage) {","        if payload.hasVideo {","            print(\"Incoming VIDEO call\")","            prepareVideoCallUI()","        } else {","            print(\"Incoming AUDIO call\")","            prepareAudioCallUI()","        }","","        CallManager.shared.storeCall(payload)","    }","","    private func prepareVideoCallUI() {","        print(\"Preparing video call UI\")","    }","","    private func prepareAudioCallUI() {","        print(\"Preparing audio call UI\")","    }","}"],"syntax":"swift"},{"text":"Handle Types","type":"heading","anchor":"Handle-Types","level":2},{"type":"heading","level":3,"anchor":"Phone-Number-Format","text":"Phone Number Format"},{"syntax":"swift","code":["Pushwoosh.VoIP.initializeVoIP(true, ringtoneSound: nil, handleTypes: 2)"],"type":"codeListing"},{"level":3,"text":"Email Format","type":"heading","anchor":"Email-Format"},{"code":["Pushwoosh.VoIP.initializeVoIP(true, ringtoneSound: nil, handleTypes: 3)"],"syntax":"swift","type":"codeListing"},{"text":"Generic Format","level":3,"type":"heading","anchor":"Generic-Format"},{"type":"codeListing","syntax":"swift","code":["Pushwoosh.VoIP.initializeVoIP(true, ringtoneSound: nil, handleTypes: 1)"]},{"anchor":"Error-Handling","text":"Error Handling","type":"heading","level":2},{"text":"Handle Provider Reset","type":"heading","level":3,"anchor":"Handle-Provider-Reset"},{"type":"codeListing","syntax":"swift","code":["extension AppDelegate: PWVoIPCallDelegate {","","    func pwProviderDidReset(_ provider: CXProvider) {","        print(\"CallKit provider reset\")","","        let allCallUUIDs = CallManager.shared.getAllCallUUIDs()","        for uuid in allCallUUIDs {","            endCallSession(uuid)","            CallManager.shared.removeCall(uuid: uuid)","        }","","        let audioSession = AVAudioSession.sharedInstance()","        try? audioSession.set