atch {","            action.fail()","            return","        }","","        startCallWithPeer(callInfo.callerName)","        action.fulfill()","    }","","    private func startCallWithPeer(_ peerName: String) {","        print(\"Starting call with \\(peerName)\")","    }","}"]},{"type":"heading","text":"End Call","level":3,"anchor":"End-Call"},{"type":"codeListing","syntax":"swift","code":["extension AppDelegate: PWVoIPCallDelegate {","","    func pwPerform(_ action: CXEndCallAction) {","        let callUUID = action.callUUID","","        endCallSession(callUUID)","        CallManager.shared.removeCall(uuid: callUUID)","","        let audioSession = AVAudioSession.sharedInstance()","        try? audioSession.setActive(false, options: .notifyOthersOnDeactivation)","","        action.fulfill()","    }","","    private func endCallSession(_ uuid: UUID) {","        print(\"Ending call: \\(uuid)\")","    }","}"]},{"anchor":"Call-Controls","text":"Call Controls","level":2,"type":"heading"},{"level":3,"type":"heading","anchor":"MuteUnmute","text":"Mute\/Unmute"},{"syntax":"swift","code":["extension AppDelegate: PWVoIPCallDelegate {","","    func pwPerform(_ action: CXSetMutedCallAction) {","        let isMuted = action.isMuted","","        if isMuted {","            disableMicrophone()","        } else {","            enableMicrophone()","        }","","        action.fulfill()","    }","","    private func disableMicrophone() {","        print(\"Microphone disabled\")","    }","","    private func enableMicrophone() {","        print(\"Microphone enabled\")","    }","}"],"type":"codeListing"},{"text":"Hold\/Resume","anchor":"HoldResume","type":"heading","level":3},{"code":["extension AppDelegate: PWVoIPCallDelegate {","","    func pwPerform(_ action: CXSetHeldCallAction) {","        let callUUID = action.callUUID","        let isOnHold = action.isOnHold","","        if isOnHold {","            pauseCall(callUUID)","        } else {","            resumeCall(callUUID)