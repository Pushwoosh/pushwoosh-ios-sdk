{"primaryContentSections":[{"kind":"content","content":[{"anchor":"Overview","type":"heading","text":"Overview","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"This page provides ready-to-use code examples for integrating Pushwoosh into your tvOS application. Examples cover basic setup, Rich Media configuration, custom push handling, and advanced scenarios."}]},{"type":"heading","anchor":"Basic-Setup","text":"Basic Setup","level":2},{"text":"Minimal AppDelegate Setup","anchor":"Minimal-AppDelegate-Setup","level":3,"type":"heading"},{"inlineContent":[{"type":"text","text":"The simplest way to integrate Pushwoosh into your tvOS app:"}],"type":"paragraph"},{"type":"codeListing","code":["import UIKit","import Pushwoosh","import PushwooshTVOS","","@main","class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                     didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","        Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","        \/\/ Register for push notifications","        Pushwoosh.TVoS.registerForTvPushNotifications()","","        return true","    }","","    func application(_ application: UIApplication,","                     didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Pushwoosh.TVoS.handleTvPushToken(deviceToken)","    }","","    func application(_ application: UIApplication,","                     didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Pushwoosh.TVoS.handleTvPushRegistrationFailure(error)","    }","","    func application(_ application: UIApplication,","                     didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                     fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","    }","}"],"syntax":"swift"},{"level":2,"type":"heading","anchor":"Rich-Media-Configuration","text":"Rich Media Configuration"},{"type":"heading","text":"Center Position with Bottom Animation","anchor":"Center-Position-with-Bottom-Animation","level":3},{"type":"paragraph","inlineContent":[{"type":"text","text":"Display Rich Media in the center with slide-in animation from bottom:"}]},{"syntax":"swift","type":"codeListing","code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Configure Rich Media","    Pushwoosh.TVoS.configureRichMediaWith(","        position: .center,","        presentAnimation: .fromBottom,","        dismissAnimation: .toBottom","    )","","    Pushwoosh.TVoS.configureCloseButton(true)","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"]},{"type":"heading","text":"Left Side Panel","level":3,"anchor":"Left-Side-Panel"},{"inlineContent":[{"type":"text","text":"Display Rich Media as a left-side panel:"}],"type":"paragraph"},{"code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .left,","    presentAnimation: .fromLeft,","    dismissAnimation: .toLeft",")"],"type":"codeListing","syntax":"swift"},{"text":"Top Banner","type":"heading","anchor":"Top-Banner","level":3},{"inlineContent":[{"text":"Display Rich Media as a top banner with slide-down animation:","type":"text"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .top,","    presentAnimation: .fromTop,","    dismissAnimation: .toTop",")"]},{"text":"No Close Button","level":3,"type":"heading","anchor":"No-Close-Button"},{"type":"paragraph","inlineContent":[{"text":"Hide the Close button if your Rich Media has custom dismiss logic:","type":"text"}]},{"code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .center,","    presentAnimation: .fromBottom,","    dismissAnimation: .toBottom",")","","Pushwoosh.TVoS.configureCloseButton(false)"],"type":"codeListing","syntax":"swift"},{"anchor":"Custom-Push-Handling","level":2,"text":"Custom Push Handling","type":"heading"},{"type":"heading","text":"Selective Rich Media Display","level":3,"anchor":"Selective-Rich-Media-Display"},{"inlineContent":[{"type":"text","text":"Display Rich Media only for specific push types:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["func application(_ application: UIApplication,","                 didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                 fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","","    \/\/ Check if push contains Rich Media","    if let rm = userInfo[\"rm\"] as? [String: Any],","       let url = rm[\"url\"] as? String {","","        \/\/ Check URL or other criteria","        if url.contains(\"special-offer\") {","            \/\/ Display Rich Media","            if Pushwoosh.TVoS.handleTVOSPush(userInfo: userInfo) {","                completionHandler(.newData)","                return","            }","        }","    }","","    \/\/ Handle as regular push","    Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","}"]},{"anchor":"Registration-with-Completion-Handler","text":"Registration with Completion Handler","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"Register with device token and handle success\/failure:"}],"type":"paragraph"},{"code":["func application(_ application: UIApplication,","                 didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","    Pushwoosh.TVoS.registerForTvPushNotifications(withToken: deviceToken) { error in","        if let error = error {","            print(\"Failed to register: \\(error.localizedDescription)\")","        } else {","            print(\"Successfully registered for push notifications\")","        }","    }","}"],"syntax":"swift","type":"codeListing"},{"type":"heading","anchor":"Unregister-from-Push-Notifications","level":2,"text":"Unregister from Push Notifications"},{"inlineContent":[{"text":"Unregister the device from receiving push notifications:","type":"text"}],"type":"paragraph"},{"code":["Pushwoosh.TVoS.unregisterForTvPushNotifications { error in","    if let error = error {","        print(\"Failed to unregister: \\(error.localizedDescription)\")","    } else {","        print(\"Successfully unregistered from push notifications\")","    }","}"],"type":"codeListing","syntax":"swift"},{"type":"heading","text":"getTags Handler","anchor":"getTags-Handler","level":2},{"inlineContent":[{"text":"Handle tags retrieved from Rich Media:","type":"text"}],"type":"paragraph"},{"type":"codeListing","code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Set getTags handler","    Pushwoosh.TVoS.setRichMediaGetTagsHandler { tags in","        print(\"Received tags from Rich Media:\")","        for (key, value) in tags {","            print(\"  \\(key): \\(value)\")","        }","    }","","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"],"syntax":"swift"}]}],"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/PushwooshTVOS\/Examples","interfaceLanguage":"swift"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/pushwooshtvos\/examples"]},{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/pushwooshtvos\/examples"]}],"metadata":{"modules":[{"name":"PushwooshTVOS"}],"role":"article","title":"Examples","roleHeading":"Article"},"schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"text":"Code samples demonstrating common tvOS integration scenari