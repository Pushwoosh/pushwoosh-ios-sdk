{"identifier":{"url":"doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/PushwooshTVOS\/Examples","interfaceLanguage":"swift"},"schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"type":"text","text":"Code samples demonstrating common tvOS integration scenarios."}],"primaryContentSections":[{"content":[{"level":2,"type":"heading","text":"Overview","anchor":"Overview"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This page provides ready-to-use code examples for integrating Pushwoosh into your tvOS application. Examples cover basic setup, Rich Media configuration, custom push handling, and advanced scenarios."}]},{"text":"Basic Setup","type":"heading","anchor":"Basic-Setup","level":2},{"type":"heading","text":"Minimal AppDelegate Setup","level":3,"anchor":"Minimal-AppDelegate-Setup"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The simplest way to integrate Pushwoosh into your tvOS app:"}]},{"type":"codeListing","syntax":"swift","code":["import UIKit","import Pushwoosh","import PushwooshTVOS","","@main","class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                     didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","        Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","        \/\/ Register for push notifications","        Pushwoosh.TVoS.registerForTvPushNotifications()","","        return true","    }","","    func application(_ application: UIApplication,","                     didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Pushwoosh.TVoS.handleTvPushToken(deviceToken)","    }","","    func application(_ application: UIApplication,","                     didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Pushwoosh.TVoS.handleTvPushRegistrationFailure(error)","    }","","    func application(_ application: UIApplication,","                     didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                     fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","    }","}"]},{"level":2,"anchor":"Rich-Media-Configuration","text":"Rich Media Configuration","type":"heading"},{"level":3,"type":"heading","anchor":"Center-Position-with-Bottom-Animation","text":"Center Position with Bottom Animation"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Display Rich Media in the center with slide-in animation from bottom:"}]},{"syntax":"swift","code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Configure Rich Media","    Pushwoosh.TVoS.configureRichMediaWith(","        position: .center,","        presentAnimation: .fromBottom,","        dismissAnimation: .toBottom","    )","","    Pushwoosh.TVoS.configureCloseButton(true)","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"],"type":"codeListing"},{"anchor":"Left-Side-Panel","text":"Left Side Panel","level":3,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Display Rich Media as a left-side panel:"}]},{"syntax":"swift","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .left,","    presentAnimation: .fromLeft,","    dismissAnimation: .toLeft",")"],"type":"codeListing"},{"anchor":"Top-Banner","level":3,"text":"Top Banner","type":"heading"},{"inlineContent":[{"type":"text","text":"Display Rich Media as a top banner with slide-down animation:"}],"type":"paragraph"},{"code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .top,","    presentAnimation: .fromTop,","    dismissAnimation: .toTop",")"],"type":"codeListing","syntax":"swift"},{"level":3,"type":"h