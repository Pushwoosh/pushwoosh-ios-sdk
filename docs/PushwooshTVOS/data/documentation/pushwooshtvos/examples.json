rt Pushwoosh","import PushwooshTVOS","","@main","class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                     didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","        Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","        \/\/ Register for push notifications","        Pushwoosh.TVoS.registerForTvPushNotifications()","","        return true","    }","","    func application(_ application: UIApplication,","                     didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Pushwoosh.TVoS.handleTvPushToken(deviceToken)","    }","","    func application(_ application: UIApplication,","                     didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Pushwoosh.TVoS.handleTvPushRegistrationFailure(error)","    }","","    func application(_ application: UIApplication,","                     didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                     fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","    }","}"]},{"text":"Rich Media Configuration","anchor":"Rich-Media-Configuration","type":"heading","level":2},{"text":"Center Position with Bottom Animation","anchor":"Center-Position-with-Bottom-Animation","type":"heading","level":3},{"type":"paragraph","inlineContent":[{"type":"text","text":"Display Rich Media in the center with slide-in animation from bottom:"}]},{"syntax":"swift","type":"codeListing","code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Configure Rich Media","    Pushwoosh.TVoS.configureRichMediaWith(","        position: .center