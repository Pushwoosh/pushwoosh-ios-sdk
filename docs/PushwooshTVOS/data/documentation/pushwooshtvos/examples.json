"class AppDelegate: UIResponder, UIApplicationDelegate {","","    func application(_ application: UIApplication,","                     didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","        Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","        \/\/ Register for push notifications","        Pushwoosh.TVoS.registerForTvPushNotifications()","","        return true","    }","","    func application(_ application: UIApplication,","                     didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","        Pushwoosh.TVoS.handleTvPushToken(deviceToken)","    }","","    func application(_ application: UIApplication,","                     didFailToRegisterForRemoteNotificationsWithError error: Error) {","        Pushwoosh.TVoS.handleTvPushRegistrationFailure(error)","    }","","    func application(_ application: UIApplication,","                     didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                     fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","        Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","    }","}"]},{"text":"Rich Media Configuration","type":"heading","level":2,"anchor":"Rich-Media-Configuration"},{"anchor":"Center-Position-with-Bottom-Animation","text":"Center Position with Bottom Animation","type":"heading","level":3},{"type":"paragraph","inlineContent":[{"text":"Display Rich Media in the center with slide-in animation from bottom:","type":"text"}]},{"type":"codeListing","code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Configure Rich Media","    Pushwoosh.TVoS.configureRichMediaWith(","        position: .center,","        presentAnimation: .fromBottom,","        dismissAnimation: .toBottom","    )","","    Pushwoosh.TVoS.configureCloseButton(true)","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"],"syntax":"swift"},{"level":3,"anchor":"Left-Side-Panel","type":"heading","text":"Left Side Panel"},{"type":"paragraph","inlineContent":[{"text":"Display Rich Media as a left-side panel:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .left,","    presentAnimation: .fromLeft,","    dismissAnimation: .toLeft",")"]},{"anchor":"Top-Banner","type":"heading","level":3,"text":"Top Banner"},{"inlineContent":[{"text":"Display Rich Media as a top banner with slide-down animation:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .top,","    presentAnimation: .fromTop,","    dismissAnimation: .toTop",")"]},{"text":"No Close Button","level":3,"anchor":"No-Close-Button","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Hide the Close button if your Rich Media has custom dismiss logic:"}]},{"code":["Pushwoosh.TVoS.configureRichMediaWith(","    position: .center,","    presentAnimation: .fromBottom,","    dismissAnimation: .toBottom",")","","Pushwoosh.TVoS.configureCloseButton(false)"],"type":"codeListing","syntax":"swift"},{"anchor":"Custom-Push-Handling","level":2,"text":"Custom Push Handling","type":"heading"},{"level":3,"text":"Selective Rich Media Display","type":"heading","anchor":"Selective-Rich-Media-Display"},{"type":"paragraph","inlineContent":[{"text":"Display Rich Media only for specific push types:","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["func application(_ application: UIApplication,","                 didReceiveRemoteNotification userInfo: [AnyHashable: Any],","                 fetchCompletionHandler completionHandler: @escaping (UIBackgroundFetchResult) -> Void) {","","    \/\/ Check if push contains Rich Media","    if let rm = userInfo[\"rm\"] as? [String: Any],","       let url = rm[\"url\"] as? String {","","        \/\/ Check URL or other criteria","        if url.contains(\"special-offer\") {","            \/\/ Display Rich Media","            if Pushwoosh.TVoS.handleTVOSPush(userInfo: userInfo) {","                completionHandler(.newData)","                return","            }","        }","    }","","    \/\/ Handle as regular push","    Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","}"]},{"anchor":"Registration-with-Completion-Handler","level":2,"type":"heading","text":"Registration with Completion Handler"},{"inlineContent":[{"text":"Register with device token and handle success\/failure:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["func application(_ application: UIApplication,","                 didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","    Pushwoosh.TVoS.registerForTvPushNotifications(withToken: deviceToken) { error in","        if let error = error {","            print(\"Failed to register: \\(error.localizedDescription)\")","        } else {","            print(\"Successfully registered for push notifications\")","        }","    }","}"]},{"anchor":"Unregister-from-Push-Notifications","text":"Unregister from Push Notifications","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"Unregister the device from receiving push notifications:","type":"text"}]},{"syntax":"swift","code":["Pushwoosh.TVoS.unregisterForTvPushNotifications { error in","    if let error = error {","        print(\"Failed to unregister: \\(error.localizedDescription)\")","    } else {","        print(\"Successfully unregistered from push notifications\")","    }","}"],"type":"codeListing"},{"text":"getTags Handler","type":"heading","level":2,"anchor":"getTags-Handler"},{"inlineContent":[{"type":"text","text":"Handle tags retrieved from Rich Media:"}],"type":"paragraph"},{"code":["func application(_ application: UIApplication,","                 didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","","    Pushwoosh.TVoS.setAppCode(\"XXXXX-XXXXX\")","","    \/\/ Set getTags handler","    Pushwoosh.TVoS.setRichMediaGetTagsHandler { tags in","        print(\"Received tags from Rich Media:\")","        for (key, value) in tags {","            print(\"  \\(key): \\(value)\")","        }","    }","","    Pushwoosh.TVoS.registerForTvPushNotifications()","","    return true","}"],"type":"codeListing","syntax":"swift"}],"kind":"content"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/PushwooshTVOS"]]},"references":{"doc://com.pushwoosh.PushwooshTVOS/documentation/PushwooshTVOS/PushwooshTVOSImplementation":{"identifier":"doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/PushwooshTVOS\/PushwooshTVOSImplementation","role":"symbol","url":"\/documentation\/pushwooshtvos\/pushwooshtvosimplementation","abstract":[{"type":"text","text":"Main implementation class for tvOS push notifications and Rich Media."}],"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"PushwooshTVOSImplementation"}],"type":"topic","fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"PushwooshTVOSImplementation"}],"title":"PushwooshTVOSImplementation"},"doc://com.pushwoosh.PushwooshTVOS/documentation/PushwooshTVOS":{"type":"topic","title":"PushwooshTVOS","role":"collection","abstract":[{"text":"Push notifications and Rich Media for tvOS apps.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/com.pushwoosh.PushwooshTVOS\/documentation\/PushwooshTVOS","url":"\/documentation\/pushwooshtvos"},"doc://com.pushwoosh.PushwooshTVOS/documentation/PushwooshTVOS/GettingStarted":{"kind":"article","abstract":[{"text":"Integrate Modal Rich Media for your tvOS app.","typ