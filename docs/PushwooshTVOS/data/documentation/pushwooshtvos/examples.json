ich Media","    if let rm = userInfo[\"rm\"] as? [String: Any],","       let url = rm[\"url\"] as? String {","","        \/\/ Check URL or other criteria","        if url.contains(\"special-offer\") {","            \/\/ Display Rich Media","            if Pushwoosh.TVoS.handleTVOSPush(userInfo: userInfo) {","                completionHandler(.newData)","                return","            }","        }","    }","","    \/\/ Handle as regular push","    Pushwoosh.TVoS.handleTvPushReceived(userInfo: userInfo, completionHandler: completionHandler)","}"],"syntax":"swift"},{"type":"heading","text":"Registration with Completion Handler","level":2,"anchor":"Registration-with-Completion-Handler"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Register with device token and handle success\/failure:"}]},{"type":"codeListing","code":["func application(_ application: UIApplication,","                 didRegisterForRemoteNotificationsWithDeviceToken deviceToken: Data) {","    Pushwoosh.TVoS.registerForTvPushNotifications(withToken: deviceToken) { error in","        if let error = error {","            print(\"Failed to register: \\(error.localizedDescription)\")","        } else {","            print(\"Successfully registered for push notifications\")","        }","    }","}"],"syntax":"swift"},{"anchor":"Unregister-from-Push-Notifications","type":"heading","text":"Unregister from Push Notifications","level":2},{"inlineContent":[{"type":"text","text":"Unregister the device from receiving push notifications:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["Pushwoosh.TVoS.unregisterForTvPushNotifications { error in","    if let error = error {","        print(\"Failed to unregister: \\(error.localizedDescription)\")","    } else {","        print(\"Successfully unregistered from push notifications\")","    }","}"]},{"type":"heading","level":2,"anchor":"getTags-Handler","text":"getTags Handler"},{"type":"paragraph","inlineContent":[{"text":"Handl